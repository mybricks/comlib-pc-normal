{
  "namespace": "mybricks.normal-pc.antd5.input-textarea",
  "title": "文本域",
  "author": "HuangQiuyun",
  "author_name": "黄秋云",
  "version": "1.1.24",
  "description": "文本域",
  "icon": "../../../../src/form-coms/input-textarea/icon.svg",
  "preview": "https://f2.eckwai.com/kos/nlav11092/u_44zp5y.91531f9e6ba88161.png",
  "data": "../../../../src/form-coms/input-textarea/data.json",
  "runtime": "./runtime.tsx",
  "editors": "../../../../src/form-coms/input-textarea/editors.tsx",
  "upgrade": "../../../../src/form-coms/input-textarea/upgrade.ts",
  "inputs": [
    {
      "id": "setValue",
      "title": "设置值",
      "desc": "设置文本域的值，并触发值变化事件输出",
      "schema": {
        "type": "string"
      },
      "rels": [
        "setValueDone"
      ]
    },
    {
      "id": "setInitialValue",
      "title": "设置初始值",
      "desc": "设置文本域的初始值，并触发初始化事件输出",
      "schema": {
        "type": "string"
      },
      "rels": [
        "setInitialValueDone"
      ]
    },
    {
      "id": "setPlaceholder",
      "title": "设置占位符",
      "desc": "设置文本域的占位符",
      "schema": {
        "type": "string"
      },
      "rels": [
        "setPlaceholderDone"
      ]
    },
    {
      "id": "validate",
      "title": "校验",
      "desc": "触发文本域的校验检查，并输出校验结果",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValidate"
      ]
    },
    {
      "id": "getValue",
      "title": "获取值",
      "desc": "获取文本域的值并输出",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValue"
      ]
    },
    {
      "id": "resetValue",
      "title": "重置值",
      "desc": "清空文本域的值",
      "schema": {
        "type": "any"
      },
      "rels": [
        "resetValueDone"
      ]
    },
    {
      "id": "setDisabled",
      "title": "设置禁用",
      "schema": {
        "type": "any"
      },
      "rels": [
        "setDisabledDone"
      ]
    },
    {
      "id": "setEnabled",
      "title": "设置启用",
      "schema": {
        "type": "any"
      },
      "rels": [
        "setEnabledDone"
      ]
    },
    {
      "id": "isEnable",
      "title": "启用/禁用",
      "schema": {
        "type": "boolean"
      },
      "rels": [
        "isEnableDone"
      ]
    },
    {
      "id": "isEditable",
      "title": "设置编辑/只读",
      "desc": "设置表单项编辑/只读态",
      "schema": {
        "type": "boolean"
      },
      "rels": [
        "isEditableDone"
      ]
    },
    {
      "id": "setAutoFocus",
      "title": "设置聚焦状态",
      "desc": "true:聚焦 false:不聚焦",
      "schema": {
        "type": "boolean"
      },
      "rels": [
        "setAutoFocusDone"
      ]
    },
    {
      "id": "setColor",
      "title": "设置字体颜色",
      "desc": "设置文本域里字体的颜色",
      "schema": {
        "type": "string"
      },
      "rels": [
        "setColorDone"
      ]
    },
    {
      "id": "setValidateInfo",
      "title": "设置校验状态",
      "desc": "设置表单项的校验状态和校验失败提示文案",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "description": "校验状态，成功/失败",
            "type": "enum",
            "items": [
              {
                "type": "string",
                "value": "success"
              },
              {
                "type": "string",
                "value": "error"
              }
            ]
          },
          "help": {
            "description": "帮助提示信息",
            "type": "string"
          }
        }
      },
      "rels": [
        "setValidateInfoDone"
      ]
    },
    {
      "id": "getCursorPos",
      "title": "获取当前光标位置",
      "desc": "当前光标位置为：文本域失焦前光标的位置或者IO方式更新文本域值后的光标位置(最后一个字符后)",
      "schema": {
        "type": "number"
      },
      "rels": [
        "returnCursorPosValue"
      ]
    }
  ],
  "outputs": [
    {
      "id": "onInitial",
      "title": "值初始化",
      "desc": "设置初始值后触发",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onChange",
      "title": "值更新",
      "desc": "值更新后，事件输出",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setAutoFocusDone",
      "title": "设置聚焦状态完成",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "onBlur",
      "title": "失去焦点",
      "desc": "失去焦点后，事件输出",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onPressEnter",
      "title": "按下回车",
      "desc": "按下回车后，事件输出",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onValidate",
      "title": "校验触发",
      "desc": "校验事件输出",
      "schema": {
        "title": "当前值",
        "type": "string"
      }
    },
    {
      "id": "returnValidate",
      "title": "校验结果",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "title": "校验状态",
            "type": "string",
            "description": "校验状态，成功/失败"
          },
          "help": {
            "title": "校验提示",
            "type": "string",
            "description": "帮助提示信息"
          }
        }
      }
    },
    {
      "id": "returnValue",
      "title": "值输出",
      "schema": {
        "type": "string",
        "description": "文本域的值"
      }
    },
    {
      "id": "setValueDone",
      "title": "设置值完成",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setInitialValueDone",
      "title": "设置初始值完成",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setPlaceholderDone",
      "title": "设置占位符完成",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "resetValueDone",
      "title": "重置完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setDisabledDone",
      "title": "禁用完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setEnabledDone",
      "title": "启用完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "isEnableDone",
      "title": "启用/禁用完成",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "isEditableDone",
      "title": "设置编辑/只读完成",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "setColorDone",
      "title": "设置字体颜色完成",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setValidateInfoDone",
      "title": "设置校验状态完成",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "descriptions": "校验状态，成功/失败",
            "type": "enum",
            "items": [
              {
                "type": "string",
                "value": "success"
              },
              {
                "type": "string",
                "value": "error"
              }
            ]
          },
          "help": {
            "descriptions": "提示信息",
            "type": "string"
          }
        }
      }
    },
    {
      "id": "returnCursorPosValue",
      "title": "光标位置输出",
      "schema": {
        "type": "number"
      }
    }
  ],
  "schema": [
    "mybricks.normal-pc.form-container/form-item",
    "*"
  ]
}