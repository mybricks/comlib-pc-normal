{
  "namespace": "mybricks.normal-pc.switch",
  "title": "开关",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "version": "1.1.18",
  "description": "开关选择器",
  "icon": "../../../../src/form-coms/switch/icon.svg",
  "preview": "https://f2.eckwai.com/kos/nlav11092/u_8969hz.276cbe33b62e0f79.png",
  "data": "../../../../src/form-coms/switch/data.json",
  "runtime": "./runtime.tsx",
  "editors": "../../../../src/form-coms/switch/editors.tsx",
  "upgrade": "../../../../src/form-coms/switch/upgrade.ts",
  "inputs": [
    {
      "id": "setValue",
      "title": "设置数据",
      "desc": "设置开关的值，并触发值变化事件输出",
      "schema": {
        "type": "boolean"
      },
      "rels": [
        "setValueDone"
      ]
    },
    {
      "id": "setInitialValue",
      "title": "设置初始值",
      "desc": "设置开关的初始值，并触发值初始化事件输出",
      "schema": {
        "type": "boolean"
      },
      "rels": [
        "setInitialValueDone"
      ]
    },
    {
      "id": "validate",
      "title": "校验",
      "desc": "触发开关的校验检查，并输出校验结果",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValidate"
      ]
    },
    {
      "id": "getValue",
      "title": "获取数据",
      "desc": "获取开关的值并输出",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValue"
      ]
    },
    {
      "id": "resetValue",
      "title": "重置数据",
      "desc": "清空开关的值",
      "schema": {
        "type": "any"
      },
      "rels": [
        "resetValueDone"
      ]
    },
    {
      "id": "setDisabled",
      "title": "设置禁用",
      "desc": "禁用开关，并触发禁用完成事件输出",
      "schema": {
        "type": "any"
      },
      "rels": [
        "setDisabledDone"
      ]
    },
    {
      "id": "setEnabled",
      "title": "设置启用",
      "desc": "启用开关，并触发启用完成事件输出",
      "schema": {
        "type": "any"
      },
      "rels": [
        "setEnabledDone"
      ]
    },
    {
      "id": "isEnable",
      "title": "启用/禁用",
      "desc": "对开关进行启用/禁用状态设置，并触发启用/禁用完成事件",
      "schema": {
        "type": "boolean"
      },
      "rels": [
        "isEnableDone"
      ]
    },
    {
      "id": "isEditable",
      "title": "设置编辑/只读",
      "desc": "设置表单项只读态",
      "schema": {
        "type": "boolean"
      },
      "rels": [
        "isEditableDone"
      ]
    },
    {
      "id": "setValidateInfo",
      "title": "设置校验状态",
      "desc": "设置表单项的校验状态和校验失败提示文案",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "type": "enum",
            "description": "校验状态，成功/失败",
            "items": [
              {
                "type": "string",
                "value": "success"
              },
              {
                "type": "string",
                "value": "error"
              }
            ]
          },
          "help": {
            "description": "帮助提示信息",
            "type": "string"
          }
        }
      },
      "rels": [
        "setValidateInfoDone"
      ]
    }
  ],
  "outputs": [
    {
      "id": "onInitial",
      "title": "值初始化",
      "desc": "开关的值初始化事件输出",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "onChange",
      "title": "值更新",
      "desc": "开关的值更新事件输出",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "onValidate",
      "title": "校验触发",
      "desc": "开关的校验触发事件输出",
      "schema": {
        "title": "当前值",
        "type": "boolean"
      }
    },
    {
      "id": "returnValidate",
      "title": "校验结果",
      "desc": "开关输出校验结果",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "title": "校验状态",
            "type": "string",
            "description": "校验状态，成功/失败"
          },
          "help": {
            "title": "校验提示",
            "type": "string",
            "description": "帮助提示信息"
          }
        }
      }
    },
    {
      "id": "returnValue",
      "title": "值输出",
      "desc": "获取开关的值后，值输出",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "setValueDone",
      "title": "设置值完成",
      "desc": "设置值事件完成输出",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "setInitialValueDone",
      "title": "设置初始值完成",
      "desc": "设置初始值事件完成输出",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "resetValueDone",
      "title": "重置完成",
      "desc": "重置事件完成输出",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setDisabledDone",
      "title": "禁用完成",
      "desc": "禁用事件完成输出",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setEnabledDone",
      "title": "启用完成",
      "desc": "启用事件完成输出",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "isEnableDone",
      "title": "启用/禁用完成",
      "desc": "启用/禁用事件完成输出",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "isEditableDone",
      "title": "设置编辑/只读完成",
      "desc": "设置编辑/只读事件完成输出",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "setValidateInfoDone",
      "title": "设置校验状态完成",
      "desc": "设置校验状态事件完成输出",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "description": "校验状态，成功/失败",
            "type": "enum",
            "items": [
              {
                "type": "string",
                "value": "success"
              },
              {
                "type": "string",
                "value": "error"
              }
            ]
          },
          "help": {
            "description": "帮助提示信息",
            "type": "string"
          }
        }
      }
    }
  ],
  "schema": [
    "mybricks.normal-pc.form-container/form-item",
    "*"
  ]
}