{
  "namespace": "mybricks.normal-pc.antd5.rate",
  "title": "评分",
  "author": "HuangQiuyun",
  "author_name": "黄秋云",
  "version": "1.1.10",
  "description": "评分",
  "icon": "../../../../src/form-coms/rate/icon.svg",
  "preview": "https://f2.eckwai.com/kos/nlav11092/u_1tias6.65a8bc3682e3ebb4.png",
  "data": "../../../../src/form-coms/rate/data.json",
  "runtime": "./runtime.tsx",
  "editors": "../../../../src/form-coms/rate/editors.ts",
  "upgrade": "../../../../src/form-coms/rate/upgrade.ts",
  "inputs": [
    {
      "id": "setValue",
      "title": "设置值",
      "desc": "设置评分的值，并触发值变化事件输出",
      "schema": {
        "type": "number"
      },
      "rels": [
        "setValueDone"
      ]
    },
    {
      "id": "setInitialValue",
      "title": "设置初始值",
      "desc": "设置评分的初始值，并触发值初始化事件输出",
      "schema": {
        "type": "number"
      },
      "rels": [
        "setInitialValueDone"
      ]
    },
    {
      "id": "validate",
      "title": "校验",
      "desc": "触发评分的校验检查，并输出校验结果",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValidate"
      ]
    },
    {
      "id": "getValue",
      "title": "获取值",
      "desc": "获取评分的值并输出",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValue"
      ]
    },
    {
      "id": "resetValue",
      "title": "重置值",
      "desc": "清空评分的值",
      "schema": {
        "type": "any"
      },
      "rels": [
        "resetValueDone"
      ]
    },
    {
      "id": "setDisabled",
      "title": "设置只读",
      "desc": "阻止用户评分",
      "schema": {
        "type": "any"
      },
      "rels": [
        "setDisabledDone"
      ]
    },
    {
      "id": "setEnabled",
      "title": "设置启用",
      "desc": "允许用户评分",
      "schema": {
        "type": "any"
      },
      "rels": [
        "setEnabledDone"
      ]
    },
    {
      "id": "isEnable",
      "title": "启用/只读",
      "desc": "根据输入值控制评分启用/只读，true-启用，false-只读",
      "schema": {
        "type": "boolean"
      },
      "rels": [
        "isEnableDone"
      ]
    },
    {
      "id": "setValidateInfo",
      "title": "设置校验状态",
      "desc": "设置表单项的校验状态和校验失败提示文案",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "type": "enum",
            "description": "校验状态",
            "items": [
              {
                "type": "string",
                "value": "success"
              },
              {
                "type": "string",
                "value": "error"
              }
            ]
          },
          "help": {
            "type": "string",
            "description": "校验提示"
          }
        }
      },
      "rels": [
        "setValidateInfoDone"
      ]
    }
  ],
  "outputs": [
    {
      "id": "onInitial",
      "title": "值初始化",
      "desc": "评分值初始化时触发，输出评分初始值",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "onChange",
      "title": "值更新",
      "desc": "评分值更新时触发，输出评分当前值",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "onValidate",
      "title": "校验触发",
      "desc": "评分校验时触发，输出评分当前值",
      "schema": {
        "title": "当前值",
        "type": "number"
      }
    },
    {
      "id": "returnValidate",
      "title": "校验结果",
      "desc": "输出评分校验结果",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "title": "校验状态",
            "type": "string",
            "description": "校验状态"
          },
          "help": {
            "title": "校验提示",
            "type": "string",
            "description": "校验提示"
          }
        }
      }
    },
    {
      "id": "returnValue",
      "title": "值输出",
      "desc": "输出评分当前值",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "setValueDone",
      "title": "设置值完成",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "setInitialValueDone",
      "title": "设置初始值完成",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "resetValueDone",
      "title": "重置完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setDisabledDone",
      "title": "只读完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setEnabledDone",
      "title": "启用完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "isEnableDone",
      "title": "启用/只读完成",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "setValidateInfoDone",
      "title": "设置校验状态完成",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "type": "enum",
            "description": "校验状态",
            "items": [
              {
                "type": "string",
                "value": "success"
              },
              {
                "type": "string",
                "value": "error"
              }
            ]
          },
          "help": {
            "type": "string",
            "description": "校验提示"
          }
        }
      }
    }
  ],
  "schema": [
    "mybricks.normal-pc.form-container/form-item",
    "*"
  ]
}