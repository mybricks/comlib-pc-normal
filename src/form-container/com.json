{
  "title": "表单容器",
  "namespace": "mybricks.normal-pc.form-container",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "version": "1.0.0",
  "description": "表单容器",
  "icon": "https://ali2.a.kwimgs.com/udata/pkg/ks-merchant/fangzhou/static_picture/form.085afdee65645d49.svg",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.ts",
  "inputs": [
    {
      "id": "initial",
      "title": "初始化表单",
      "schema": {
        "type": "object",
        "properties": {}
      }
    },
    {
      "id": "setFieldsValue",
      "title": "设置表单值",
      "schema": {
        "type": "object",
        "properties": {}
      }
    },
    {
      "id": "submit",
      "title": "提交表单",
      "rels": ["onFinish"],
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "resetFields",
      "title": "重置表单",
      "rels": ["onResetFinish"],
      "schema": {
        "type": "any"
      }
    }
  ],
  "outputs": [
    {
      "id": "onFinish",
      "title": "提交数据输出",
      "schema": {
        "type": "object",
        "properties": {}
      }
    },
    {
      "id": "onInitial",
      "title": "初始化表单",
      "schema": {
        "type": "object",
        "properties": {
          "values": {
            "title": "表单值",
            "type": "object",
            "properties": {}
          },
          "formInstance": {
            "title": "表单实例",
            "type": "object",
            "properties": {}
          }
        }
      }
    },
    {
      "id": "onResetFinish",
      "title": "重置完成",
      "schema": {
        "type": "any"
      }
    }
  ],
  "slots": [
    {
      "id": "content",
      "title": "拖入表单项",
      "schema": "mybricks.normal-pc.form-container/form-item"
    }
  ]
}
