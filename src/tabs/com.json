{
  "title": "标签页",
  "namespace": "mybricks.normal-pc.tabs",
  "version": "1.0.24",
  "description": "标签页组件",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "icon": "./icon.svg",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editor/index.ts",
  "upgrade": "./upgrade.ts",
  "target": {
    "toReact": "./toReact.ts"
  },
  "inputs": [
    {
      "id": "previous",
      "title": "上一页",
      "schema": {
        "type": "any"
      },
      "rels": [
        "previousComplete"
      ],
      "desc": "标签页上一页"
    },
    {
      "id": "next",
      "title": "下一页",
      "schema": {
        "type": "any"
      },
      "rels": [
        "nextComplete"
      ],
      "desc": "标签页下一页"
    },
    {
      "id": "active",
      "title": "激活tab项",
      "schema": {
        "type": "number"
      },
      "rels": [
        "activeComplete"
      ],
      "desc": "标签页下标，从0开始计数"
    },
    {
      "id": "outActiveTab",
      "title": "获取激活tab项",
      "rels": [
        "outActiveTab"
      ],
      "schema": {
        "type": "any"
      },
      "desc": "获取当前显示的标签页信息"
    },
    {
      "id": "getTabs",
      "title": "获取标签页列表",
      "schema": {
        "type": "any"
      },
      "rels": [
        "getTabsDone"
      ]
    }
  ],
  "outputs": [
    {
      "id": "tabClick",
      "title": "标签页点击",
      "schema": {
        "type": "object",
        "properties": {
          "id": {
            "title": "标签页id",
            "type": "string"
          },
          "name": {
            "title": "标签页标题",
            "type": "string"
          },
          "key": {
            "title": "标签页key",
            "type": "string"
          },
          "index": {
            "title": "下标",
            "type": "number"
          }
        }
      }
    },
    {
      "id": "outActiveTab",
      "title": "激活tab项输出",
      "schema": {
        "type": "object",
        "properties": {
          "id": {
            "title": "标签页id",
            "type": "string"
          },
          "name": {
            "title": "标签页标题",
            "type": "string"
          },
          "key": {
            "title": "标签页key",
            "type": "string"
          },
          "index": {
            "title": "下标",
            "type": "number"
          }
        }
      }
    },
    {
      "id": "tab0_into",
      "title": "标签页1显示",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "tab0_leave",
      "title": "标签页1隐藏",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "previousComplete",
      "title": "完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "nextComplete",
      "title": "完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "activeComplete",
      "title": "完成",
      "schema": {
        "type": "any"
      }
    }, 
    {
      "id": "addTab",
      "title": "标签页新增",
      "schema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "id": {
              "title": "标签页id",
              "type": "string"
            },
            "name": {
              "title": "标签页标题",
              "type": "string"
            },
            "key": {
              "title": "标签页key",
              "type": "string"
            },
            "index": {
              "title": "下标",
              "type": "number"
            },
            "closable": {
              "title": "可关闭",
              "type": "boolean"
            }
          }
        }
      }
    },
    {
      "id": "removeTab",
      "title": "标签页删除",
      "schema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "id": {
              "title": "标签页id",
              "type": "string"
            },
            "name": {
              "title": "标签页标题",
              "type": "string"
            },
            "key": {
              "title": "标签页key",
              "type": "string"
            },
            "index": {
              "title": "下标",
              "type": "number"
            },
            "closable": {
              "title": "可关闭",
              "type": "boolean"
            }
          }
        }
      }
    },
    {
      "id": "getTabsDone",
      "title": "标签页数据",
      "schema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "id": {
              "title": "标签页id",
              "type": "string"
            },
            "name": {
              "title": "标签页标题",
              "type": "string"
            },
            "key": {
              "title": "标签页key",
              "type": "string"
            }
          }
        }
      }
    }
  ],
  "slots": [
    {
      "id": "tab0",
      "title": "标签页1",
      "layout": "smart"
    }
  ]
}