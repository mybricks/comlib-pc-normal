{
  "title": "数据表格",
  "namespace": "mybricks.normal-pc.table",
  "version": "1.0.14",
  "description": "数据表格",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "icon": "./icon.png",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors/index.tsx",
  "upgrade": "./upgrade.ts",
  "target": {
    "toReact": "./toReact.ts"
  },
  "inputs": [
    {
      "id": "dataSource",
      "title": "设置数据源",
      "schema": {
        "type": "array"
      },
      "desc": "设置表格数据"
    },
    {
      "id": "getTableData",
      "title": "获取表格数据",
      "schema": {
        "type": "any"
      },
      "rels": [
        "getTableData"
      ],
      "desc": "获取表格数据并输出"
    },
    {
      "id": "startLoading",
      "title": "开启loading",
      "schema": {
        "type": "any"
      },
      "desc": "设置表格为loading状态"
    },
    {
      "id": "endLoading",
      "title": "关闭loading",
      "schema": {
        "type": "any"
      },
      "desc": "关闭表格loading状态"
    }
  ],
  "outputs": [
    {
      "id": "getTableData",
      "title": "表格数据",
      "schema": {
        "type": "array"
      }
    }
  ]
}