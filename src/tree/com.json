{
  "title": "树",
  "namespace": "mybricks.normal-pc.tree",
  "version": "1.0.62",
  "description": "树状图",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "icon": "./icon.svg",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.tsx",
  "upgrade": "./upgrade.ts",
  "inputs": [
    {
      "id": "treeData",
      "title": "输入数据",
      "desc": "树组件的节点数据，确保key唯一",
      "schema": {
        "title": "树组件数据",
        "type": "array",
        "description": "数组格式的数据节后，需要包含以下字段title, key等字段",
        "items": {
          "type": "object",
          "properties": {
            "title": {
              "title": "标题",
              "type": "string"
            },
            "key": {
              "title": "字段名",
              "type": "string"
            },
            "disableCheckbox": {
              "title": "禁用勾选",
              "type": "boolean",
              "description": "勾选场景下，可以带上这个字段"
            },
            "children": {
              "title": "子项",
              "type": "array",
              "description": "节点子项的数据",
              "items": {
                "type": "object"
              }
            }
          }
        }
      },
      "rels": [
        "setTreeDataDone"
      ]
    },
    {
      "id": "nodeData",
      "title": "更新节点数据",
      "desc": "要更新的节点数据",
      "schema": {
        "type": "object",
        "description": "节点数据格式",
        "properties": {
          "title": {
            "type": "string"
          },
          "children": {
            "title": "子项",
            "type": "array",
            "items": {
              "type": "object"
            }
          }
        }
      },
      "rels": [
        "setNodeDataDone"
      ]
    },
    {
      "id": "searchValue",
      "title": "搜索",
      "schema": {
        "type": "string"
      },
      "desc": "输入字符串，高亮展示匹配的树节点",
      "rels": [
        "searchValueDone"
      ]
    },
    {
      "id": "filter",
      "title": "过滤",
      "schema": {
        "type": "string"
      },
      "desc": "输入字符串，筛选匹配的树节点",
      "rels": [
        "filterDone"
      ]
    },
    {
      "id": "setSelectedKeys",
      "title": "设置选中节点",
      "schema": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "desc": "输入节点key数组, 设置树形结构中选中的节点",
      "rels": [
        "setSelectedKeysDone"
      ]
    },
    {
      "id": "setExpandedKeys",
      "title": "设置展开节点",
      "schema": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "desc": "输入节点key数组, 设置树形结构中展开的节点列表",
      "rels": [
        "setExpandedKeysDone"
      ]
    },
    {
      "id": "setOpenDepth",
      "title": "设置展开深度",
      "desc": "设置树的展开深度: 0表示全部折叠, -1表示全部展开",
      "schema": {
        "type": "number"
      },
      "rels": [
        "setOpenDepthDone"
      ]
    },
    {
      "id": "setDragConfig",
      "title": "设置拖拽功能",
      "desc": "配置树组件的拖拽功能",
      "schema": {
        "type": "object",
        "properties": {
          "draggable": {
            "description": "允许拖拽属性，可以为布尔值或者'custom'自定义节点",
            "type": "enum",
            "items": [
              {
                "type": "boolean"
              },
              {
                "type": "string",
                "value": "custom"
              }
            ]
          },
          "draggableScript": {
            "type": "string",
            "description": "节点可拖拽表达式,根据表达式动态计算是否可拖拽"
          },
          "allowDrop": {
            "type": "enum",
            "desc": "允许放置配置，可以为布尔值或者'custom'自定义节点",
            "items": [
              {
                "type": "boolean"
              },
              {
                "type": "string",
                "value": "custom"
              }
            ]
          },
          "allowDropScript": {
            "type": "string",
            "description": "节点可放置表达式,根据表达式动态计算是否可放置"
          },
          "useDropScope": {
            "type": "enum",
            "description": "放置范围限制,false表示无限制，parent表示父节点",
            "items": [
              {
                "type": "boolean",
                "value": false
              },
              {
                "type": "string",
                "value": "parent"
              }
            ]
          },
          "dropScopeMessage": {
            "type": "string",
            "description": "禁止放置提示语"
          }
        }
      },
      "rels": [
        "setDragConfigDone"
      ]
    },
    {
      "id": "getTreeData",
      "title": "获取数据",
      "desc": "获取树的当前数据并输出",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnTreeData"
      ]
    },
    {
      "id": "getSelectedKeys",
      "title": "获取选中节点数据",
      "schema": {
        "type": "any"
      },
      "desc": "输出当前选中节点的数据",
      "rels": [
        "returnSelectedKeys"
      ]
    }
  ],
  "outputs": [
    {
      "id": "onChange",
      "title": "数据变化",
      "desc": "更改树的数据出，触发数据变化输出",
      "schema": {
        "title": "树组件数据",
        "type": "array",
        "description": "树的数据格式",
        "items": {
          "type": "object",
          "properties": {
            "title": {
              "title": "标题",
              "type": "string"
            },
            "key": {
              "title": "字段名",
              "type": "string"
            },
            "children": {
              "title": "子项",
              "type": "array",
              "items": {
                "type": "object"
              }
            }
          }
        }
      }
    },
    {
      "id": "returnTreeData",
      "title": "数据输出",
      "desc": "获取树的数据后，输出数据",
      "schema": {
        "title": "树组件数据",
        "type": "array",
        "description": "树的数据格式",
        "items": {
          "type": "object",
          "properties": {
            "title": {
              "title": "标题",
              "type": "string"
            },
            "key": {
              "title": "字段名",
              "type": "string"
            },
            "children": {
              "title": "子项",
              "type": "array",
              "items": {
                "type": "object"
              }
            }
          }
        }
      }
    },
    {
      "id": "submit",
      "title": "勾选项数据输出",
      "desc": "支持勾选的场景下，获取勾选的数据后，勾选项数据输出",
      "schema": {
        "type": "array",
        "description": "所有勾选的节点的key",
        "items": {
          "type": "string"
        }
      }
    },
    {
      "id": "returnSelectedKeys",
      "title": "选中节点数据输出",
      "desc": "选中的数据",
      "schema": {
        "type": "array",
        "desc": "数组结构的数据，里面包含节点的key",
        "items": {
          "type": "string"
        }
      }
    },
    {
      "id": "click",
      "title": "节点选中事件",
      "desc": "节点选中后，事件输出",
      "schema": {
        "type": "array",
        "description": "所有选中的节点的key",
        "items": {
          "type": "string"
        }
      }
    },
    {
      "id": "addNodeDone",
      "title": "添加完成",
      "desc": "在开启'可添加节点'条件下，添加节点后，事件输出",
      "schema": {
        "type": "object",
        "description": "新增节点的数据结构",
        "properties": {
          "node": {
            "type": "object",
            "title": "新增节点数据",
            "description": "新增节点数据格式",
            "properties": {
              "title": {
                "type": "string"
              },
              "value": {
                "type": "string"
              },
              "key": {
                "type": "string"
              }
            }
          },
          "parent": {
            "type": "object",
            "title": "新增节点的父节点数据"
          }
        }
      }
    },
    {
      "id": "modify",
      "title": "修改",
      "desc": "修改节点数据",
      "schema": {
        "title": "节点数据",
        "type": "object",
        "description": "数据格式",
        "properties": {
          "title": {
            "title": "标题",
            "type": "string"
          },
          "key": {
            "title": "字段名",
            "type": "string"
          },
          "value": {
            "title": "值",
            "type": "string"
          },
          "disableCheckbox": {
            "title": "禁用勾选",
            "type": "boolean"
          }
        }
      }
    },
    {
      "id": "delete",
      "title": "删除",
      "desc": "点击删除，输出节点内容",
      "schema": {
        "title": "节点数据",
        "type": "object",
        "description": "返回删除的节点数据",
        "properties": {
          "title": {
            "title": "标题",
            "type": "string"
          },
          "key": {
            "title": "字段名",
            "type": "string"
          },
          "value": {
            "title": "值",
            "type": "string"
          },
          "disableCheckbox": {
            "title": "禁用勾选",
            "type": "boolean"
          }
        }
      }
    },
    {
      "id": "onDropDone",
      "title": "拖拽完成",
      "desc": "拖拽完成后，输出拖拽相关节点信息",
      "schema": {
        "type": "object",
        "properties": {
          "description": "拖拽节点信息",
          "dragNodeInfo": {
            "title": "拖拽节点信息",
            "type": "object",
            "properties": {
              "parent": {
                "type": "object",
                "properties": {
                  "title": {
                    "title": "标题",
                    "type": "string"
                  },
                  "key": {
                    "title": "字段名",
                    "type": "string"
                  }
                }
              },
              "node": {
                "type": "object",
                "properties": {
                  "title": {
                    "title": "标题",
                    "type": "string"
                  },
                  "key": {
                    "title": "字段名",
                    "type": "string"
                  }
                }
              },
              "index": {
                "type": "number"
              }
            }
          },
          "dropNodeInfo": {
            "title": "放置节点信息",
            "description": "放置位置节点信息",
            "type": "object",
            "properties": {
              "parent": {
                "type": "object",
                "properties": {
                  "title": {
                    "title": "标题",
                    "type": "string"
                  },
                  "key": {
                    "title": "字段名",
                    "type": "string"
                  }
                }
              },
              "node": {
                "type": "object",
                "properties": {
                  "title": {
                    "title": "标题",
                    "type": "string"
                  },
                  "key": {
                    "title": "字段名",
                    "type": "string"
                  }
                }
              },
              "index": {
                "type": "number"
              }
            }
          },
          "flag": {
            "title": "标识",
            "type": "enum",
            "description": "相对放置节点位置标识",
            "items": [
              {
                "type": "number",
                "value": 0
              },
              {
                "type": "number",
                "value": 1
              },
              {
                "type": "number",
                "value": -1
              }
            ]
          },
          "treeData": {
            "title": "拖拽后的树组件数据",
            "type": "array",
            "description": "拖拽后的树组件数据",
            "items": {
              "type": "object",
              "properties": {
                "title": {
                  "title": "标题",
                  "type": "string"
                },
                "key": {
                  "title": "字段名",
                  "type": "string"
                },
                "children": {
                  "title": "子项",
                  "type": "array",
                  "items": {
                    "type": "object"
                  }
                }
              }
            }
          }
        }
      }
    },
    {
      "id": "setTreeDataDone",
      "title": "输入数据完成",
      "desc": "设置树的数据后，输出设置的数据",
      "schema": {
        "title": "树组件数据",
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "title": {
              "title": "标题",
              "type": "string"
            },
            "key": {
              "title": "字段名",
              "type": "string"
            },
            "disableCheckbox": {
              "title": "禁用勾选",
              "type": "boolean"
            },
            "children": {
              "title": "子项",
              "type": "array",
              "items": {
                "type": "object"
              }
            }
          }
        }
      }
    },
    {
      "id": "setNodeDataDone",
      "title": "更新节点数据完成",
      "desc": "更新节点数据后，输出",
      "schema": {
        "type": "object",
        "description": "更新项内容",
        "properties": {
          "title": {
            "type": "string"
          },
          "children": {
            "title": "子项",
            "type": "array",
            "items": {
              "type": "object"
            }
          }
        }
      }
    },
    {
      "id": "searchValueDone",
      "title": "搜索完成",
      "desc": "搜索后，事件输出",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "filterDone",
      "title": "过滤完成",
      "desc": "过滤后，事件输出",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setSelectedKeysDone",
      "title": "设置选中节点完成",
      "desc": "设置选中节点后，输出选中的节点key",
      "schema": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    {
      "id": "setExpandedKeysDone",
      "title": "设置展开节点完成",
      "desc": "设置展开节点后，输出展开的节点key",
      "schema": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    {
      "id": "setOpenDepthDone",
      "title": "设置展开深度完成",
      "desc": "设置展开深度完成并输出",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "setDragConfigDone",
      "title": "设置拖拽功能完成",
      "desc": "设置拖拽配置完成后，并输出配置内容",
      "schema": {
        "type": "object",
        "properties": {
          "draggable": {
            "description": "允许拖拽属性，可以为布尔值或者'custom'自定义节点",
            "type": "enum",
            "items": [
              {
                "type": "boolean"
              },
              {
                "type": "string",
                "value": "custom"
              }
            ]
          },
          "draggableScript": {
            "type": "string",
            "description": "节点可拖拽表达式,根据表达式动态计算是否可拖拽"
          },
          "allowDrop": {
            "type": "enum",
            "desc": "允许放置配置，可以为布尔值或者'custom'自定义节点",
            "items": [
              {
                "type": "boolean"
              },
              {
                "type": "string",
                "value": "custom"
              }
            ]
          },
          "allowDropScript": {
            "type": "string",
            "description": "节点可放置表达式,根据表达式动态计算是否可放置"
          },
          "useDropScope": {
            "type": "enum",
            "description": "放置范围限制,false表示无限制，parent表示父节点",
            "items": [
              {
                "type": "boolean",
                "value": false
              },
              {
                "type": "string",
                "value": "parent"
              }
            ]
          },
          "dropScopeMessage": {
            "type": "string",
            "description": "禁止放置提示语"
          }
        }
      }
    }
  ]
}