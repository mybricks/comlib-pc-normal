{
  "title": "树组件",
  "namespace": "mybricks.normal-pc.tree",
  "version": "1.0.2",
  "description": "树状图",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "icon": "./icon.svg",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.tsx",
  "inputs": [
    {
      "id": "treeData",
      "title": "输入数据",
      "schema": {
        "title": "树组件数据",
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "title": {
              "title": "标题",
              "type": "string"
            },
            "key": {
              "title": "字段名",
              "type": "string"
            },
            "disableCheckbox": {
              "title": "禁用勾选",
              "type": "boolean"
            },
            "children": {
              "title": "子项",
              "type":"array",
              "items": {
                "type": "object"
              }
            }
          }
        }
      }
    },
    {
      "id": "checkedValues",
      "title": "设置勾选项",
      "schema": {
        "type": "array",
        "items": {
          "title": "字段名",
          "type": "string"
        }
      }
    },
    {
      "id": "nodeData",
      "title": "更新节点数据",
      "schema": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "children": {
            "title": "子项",
            "type":"array",
            "items": {
              "type": "object"
            }
          }
        }
      }
    },
    {
      "id": "searchValue",
      "title": "搜索",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "disableCheckbox",
      "title": "禁用勾选",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "enableCheckbox",
      "title": "启用勾选",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "submit",
      "title": "获取勾选项数据",
      "rels": [
        "submit"
      ],
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "addTips",
      "title": "设置添加提示文案",
      "schema": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    }
  ],
  "outputs": [
    {
      "id": "submit",
      "title": "勾选项数据输出",
      "schema": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    {
      "id": "click",
      "title": "点击事件",
      "schema": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    {
      "id": "addNodeDone",
      "title": "添加完成",
      "schema": {
        "type": "object",
        "properties": {
          "node": {
            "type": "object",
            "title": "新增节点数据",
            "properties": {
              "title": {
                "type": "string"
              },
              "value": {
                "type": "string"
              },
              "key": {
                "type": "string"
              }
            }
          },
          "parent": {
            "type": "object",
            "title": "新增节点的父节点数据"
          }
        }
      }
    },
    {
      "id": "modify",
      "title": "修改",
      "schema": {
        "title": "节点数据",
        "type": "object",
        "properties": {
          "title": {
            "title": "标题",
            "type": "string"
          },
          "key": {
            "title": "字段名",
            "type": "string"
          },
          "value": {
            "title": "值",
            "type": "string"
          },
          "disableCheckbox": {
            "title": "禁用勾选",
            "type": "boolean"
          }
        }
      }
    },
    {
      "id": "delete",
      "title": "删除",
      "schema": {
        "title": "节点数据",
        "type": "object",
        "properties": {
          "title": {
            "title": "标题",
            "type": "string"
          },
          "key": {
            "title": "字段名",
            "type": "string"
          },
          "value": {
            "title": "值",
            "type": "string"
          },
          "disableCheckbox": {
            "title": "禁用勾选",
            "type": "boolean"
          }
        }
      }
    }
  ]
}