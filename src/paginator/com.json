{
  "title": "分页器",
  "namespace": "mybricks.normal-pc.paginator",
  "version": "1.0.11",
  "description": "分页器",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "icon": "./icon.svg",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.tsx",
  "upgrade": "./upgrade.ts",
  "inputs": [
    {
      "id": "setTotal",
      "title": "设置数据总数",
      "rels": [
        "setTotalDone"
      ],
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "setPageNum",
      "title": "设置当前页码",
      "rels": [
        "setPageNumDone"
      ],
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "getPageInfo",
      "title": "获取分页数据",
      "rels": [
        "getPageInfo"
      ],
      "schema": {
        "type": "any"
      }
    }
  ],
  "outputs": [
    {
      "id": "pageChange",
      "title": "点击分页",
      "schema": {
        "type": "object",
        "properties": {
          "pageNum": {
            "type": "number"
          },
          "pageSize": {
            "type": "number"
          }
        }
      }
    },
    {
      "id": "setTotalDone",
      "title": "数据总数",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "setPageNumDone",
      "title": "当前页码",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "getPageInfo",
      "title": "分页数据",
      "schema": {
        "type": "object",
        "properties": {
          "pageNum": {
            "type": "number"
          },
          "pageSize": {
            "type": "number"
          }
        }
      }
    }
  ]
}