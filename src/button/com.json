{
  "title": "按钮",
  "namespace": "mybricks.normal-pc.custom-button",
  "version": "1.0.37",
  "description": "按钮",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "icon": "./icon.svg",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editor/index.tsx",
  "upgrade": "./upgrade.ts",
  "ai": "./ai.ts",
  "target": {
    "toReact": "./toReact.ts"
  },
  "inputs": [
    {
      "id": "dynamicTitle",
      "title": "设置标题",
      "schema": {
        "type": "string"
      },
      "rels": [
        "setDynamicTitleDone"
      ]
    },
    {
      "id": "dynamicDisabled",
      "title": "设置状态：禁用",
      "schema": {
        "type": "boolean"
      },
      "rels": [
        "setDynamicDisabledDone"
      ]
    },
    {
      "id": "setStateRunning",
      "title": "设置状态：运行中",
      "description": "设置按钮为运行中状态，可传递显示按钮的文案",
      "schema": {
        "type": "string"
      },
      "rels": [
        "setDynamicDisabledDone"
      ]
    },
    {
      "id": "setStateNormal",
      "title": "恢复",
      "description": "设置按钮为常规状态，可传递显示按钮的文案",
      "schema": {
        "type": "string"
      },
      "rels": [
        "setDynamicDisabledDone"
      ]
    },
    {
      "id": "setDynamicStyle",
      "title": "设置动态样式",
      "schema": {
        "type": "object",
        "properties": {
          "background": {
            "type": "string"
          }
        }
      },
      "rels": [
        "setDynamicStyleDone"
      ]
    }
  ],
  "outputs": [
    {
      "id": "click",
      "title": "单击",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "clickOnRunning",
      "title": "运行时单击",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "dbClick",
      "title": "双击",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "setDynamicTitleDone",
      "title": "设置标题完成",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setDynamicDisabledDone",
      "title": "设置禁用完成",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "setDynamicStyle",
      "title": "设置动态样式完成",
      "schema": {
        "type": "any"
      }
    }
  ]
}