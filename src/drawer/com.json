{
  "title": "抽屉",
  "enable": true,
  "namespace": "mybricks.normal-pc.drawer",
  "version": "1.0.0",
  "description": "抽屉从父窗体边缘滑入，覆盖住部分父窗体内容。用户在抽屉内操作时不必离开当前任务，操作完成后，可以平滑地回到原任务。",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "icon": "./icon.svg",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.tsx",
  "inputs": [
    {
      "id": "close",
      "title": "关闭",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "open",
      "title": "打开",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "title",
      "title": "修改标题",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "inputData",
      "title": "接收数据",
      "schema": {
        "type": "any"
      }
    }
  ],
  "outputs": [
    {
      "id": "cancel",
      "title": "关闭回调",
      "schema": {
        "type": "any"
      }
    }
  ],
  "slots": [
    {
      "id": "content",
      "title": "抽屉内容"
    },
    {
      "id": "footer",
      "title": "抽屉页脚"
    }
  ]
}