{
  "title": "加载中",
  "namespace": "amc.normal-h5.spin",
  "version": "1.0.7",
  "description": "用于页面和区块的加载中状态",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "icon": "./icon.png",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.tsx",
  "upgrade": "./upgrade.ts",
  "inputs": [
    {
      "id": "openLoading",
      "title": "开始加载",
      "schema": {
        "type": "any"
      },
      "rels": [
        "openLoadingDone"
      ]
    },
    {
      "id": "closeLoading",
      "title": "结束加载",
      "schema": {
        "type": "any"
      },
      "rels": [
        "closeLoadingDone"
      ]
    }
  ],
  "outputs": [
    {
      "id": "openLoadingDone",
      "title": "开始加载后",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "closeLoadingDone",
      "title": "结束加载后",
      "schema": {
        "type": "any"
      }
    }
  ],
  "slots": [
    {
      "id": "content",
      "title": "内容"
    }
  ]
}