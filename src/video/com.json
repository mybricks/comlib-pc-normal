{
  "title": "播放器",
  "namespace": "mybricks.normal-pc.video",
  "author": "tangxiaoxin",
  "author_name": "tangxiaoxin",
  "version": "1.0.4",
  "description": "播放器",
  "docs": "组件使用说明文档链接",
  "icon": "./icon.svg",
  "runtime": "./runtime.tsx",
  "data": "./data.json",
  "editors": "./editors.ts",
  "upgrade": "./upgrade.ts",
  "inputs": [
    {
      "id": "link",
      "title": "视频链接",
      "desc": "设置播放视频链接，支持mp4，mov，hls，flv视频（流）格式",
      "schema": {
        "type": "string"
      },
      "rels": ["setLinkComplete"]
    },
    {
      "id": "screenshot",
      "title": "截屏",
      "desc": "获取当前视频画面截图",
      "schema": {
        "type": "string",
        "title": "截图文件名"
      },
      "rels": ["screenshotComplete"]
    },
    {
      "id": "setPoster",
      "title": "设置封面",
      "desc": "设置播放器封面",
      "schema": {
        "type": "string",
        "title": "封面"
      },
      "rels": ["setPosterComplete"]
    }
  ],
  "outputs": [
    {
      "id": "setLinkComplete",
      "desc": "设置播放器链接后，事件输出",
      "title": "完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "screenshotComplete",
      "title": "完成",
      "desc": "截屏完成后，事件输出",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setPosterComplete",
      "title": "完成",
      "desc": "设置播放器封面后，事件输出",
      "schema": {
        "type": "any"
      }
    }
  ],
  "slots": []
}