{
  "title": "导航菜单",
  "namespace": "mybricks.normal-pc.menu",
  "version": "1.0.23",
  "description": "为页面和功能提供导航的菜单列表",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "icon": "./icon.svg",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editor/index.tsx",
  "target": {
    "toReact": "./toReact.ts"
  },
  "upgrade": "./upgrade.ts",
  "outputs": [
    {
      "id": "click",
      "title": "点击",
      "desc": "点击当前菜单项, 输出内容",
      "schema": {
        "type": "object",
        "properties": {
          "title": {
            "title": "标题",
            "description": "点击的菜单项标题",
            "type": "string"
          },
          "key": {
            "title": "唯一标识",
            "description": "点击的菜单项标识key",
            "type": "string"
          },
          "menuType": {
            "title": "类型",
            "type": "string",
            "description": "点击的菜单项类型"
          },
          "value": {
            "title": "其他数据",
            "description": "点击的菜单项的其他数据",
            "type": "any"
          }
        }
      }
    },
    {
      "id": "getActiveItem",
      "title": "获取选中项",
      "desc": "获取选中菜单项, 并输出内容",
      "schema": {
        "type": "object",
        "properties": {
          "title": {
            "title": "标题",
            "description": "选中的的菜单项标题",
            "type": "string"
          },
          "key": {
            "title": "唯一标识",
            "description": "选中的菜单项的唯一标识",
            "type": "string"
          },
          "menuType": {
            "title": "类型",
            "type": "string",
            "description": "选中的菜单项类型"
          },
          "value": {
            "title": "其他数据",
            "description": "选中的菜单项的其他数据",
            "type": "any"
          }
        }
      }
    },
    {
      "id": "setMenuDataDone",
      "title": "设置数据完成",
      "desc": "设置菜单数据输入, 触发设置数据完成事件",
      "schema": {
        "title": "菜单数据",
        "type": "object",
        "properties": {
          "dataSource": {
            "title": "菜单数据",
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "title": {
                  "title": "标题",
                  "description": "菜单项标题",
                  "type": "string"
                },
                "key": {
                  "title": "唯一标识",
                  "description": "菜单项标标识",
                  "type": "string"
                },
                "menuType": {
                  "title": "类型",
                  "type": "string",
                  "description": "菜单项类型"
                },
                "value": {
                  "title": "其他数据",
                  "description": "菜单项的其他数据",
                  "type": "any"
                }
              }
            }
          },
          "defaultActive": {
            "title": "默认选中项",
            "type": "string"
          }
        }
      }
    },
    {
      "id": "setActiveItemDone",
      "title": "设置选中项完成",
      "desc": "设置菜单选中项, 触发设置选中项完成事件",
      "schema": {
        "title": "菜单名称/唯一标识",
        "type": "string"
      }
    }
  ],
  "inputs": [
    {
      "id": "setMenuData",
      "title": "设置数据",
      "desc": "设置初始数据和选中项",
      "schema": {
        "title": "菜单数据",
        "type": "object",
        "properties": {
          "dataSource": {
            "title": "菜单数据",
            "type": "array",
            "description": "导航菜单数据",
            "items": {
              "type": "object",
              "properties": {
                "title": {
                  "title": "标题",
                  "description": "菜单项标题",
                  "type": "string"
                },
                "key": {
                  "title": "唯一标识",
                  "description": "菜单项标识",
                  "type": "string"
                },
                "menuType": {
                  "title": "类型",
                  "type": "string",
                  "description": "菜单项类型，可以为普通菜单项menu、子菜单subMenu, 分组菜单group"
                },
                "children": {
                  "title": "子菜单数据",
                  "description": "自菜单的数据，数组结构，内容和菜单项一致",
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {}
                  }
                },
                "value": {
                  "title": "其他数据",
                  "type": "string"
                }
              }
            }
          },
          "defaultActive": {
            "title": "默认选中项",
            "description": "默认选中的菜单，这里传菜单的key",
            "type": "string"
          }
        }
      },
      "rels": [
        "setMenuDataDone"
      ]
    },
    {
      "id": "setActiveItem",
      "title": "设置选中项",
      "desc": "设置默认选中菜单项",
      "schema": {
        "title": "菜单名称/唯一标识",
        "type": "string"
      },
      "rels": [
        "setActiveItemDone"
      ]
    },
    {
      "id": "getActiveItem",
      "title": "获取选中项",
      "desc": "获取选中菜单项",
      "rels": [
        "getActiveItem"
      ],
      "schema": {
        "type": "any"
      }
    }
  ]
}