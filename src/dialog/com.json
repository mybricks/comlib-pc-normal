{
  "title": "对话框",
  "namespace": "mybricks.normal-pc.dialog",
  "version": "1.0.0",
  "description": "对话框",
  "author": "ZhuLin",
  "author_name": "朱琳",
  "icon": "./icon.svg",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.ts",
  "inputs": [
    {
      "id": "open",
      "title": "打开",
      "schema": {
        "type": "follow"
      }
    },
    {
      "id": "close",
      "title": "关闭",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "title",
      "title": "修改标题",
      "schema": {
        "type": "string"
      }
    }
  ],
  "outputs": [
    {
      "id": "ok",
      "title": "确认",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "cancelBtn",
      "title": "取消",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "okClick",
      "title": "点击确认",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "cancelBtnClick",
      "title": "点击取消",
      "schema": {
        "type": "any"
      }
    }
  ],
  "slots": [
    {
      "id": "container",
      "title": "对话框容器",
      "type": "scope",
      "inputs": [
        {
          "id": "dataSource",
          "title": "输入数据",
          "schema": {
            "type": "any"
          }
        }
      ],
      "outputs": [
        {
          "id": "cancelBtn",
          "title": "取消",
          "schema": {
            "type": "follow"
          }
        },
        {
          "id": "ok",
          "title": "确认",
          "schema": {
            "type": "follow"
          }
        }
      ]
    }
  ],
  "rtType": "popup"
}
