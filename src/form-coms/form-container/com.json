{
  "title": "表单容器",
  "namespace": "mybricks.normal-pc.form-container",
  "author": "ZhuPengqiang",
  "author_name": "朱鹏强",
  "version": "1.2.3",
  "description": "表单容器",
  "icon": "./icon.svg",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors/index.ts",
  "upgrade": "./upgrade.ts",
  "ai": "./ai.ts",
  "target": {
    "toReact": "./toReact.ts"
  },
  "inputs": [
    {
      "id": "setFieldsValue",
      "title": "设置表单数据（触发值变化）",
      "schema": {
        "type": "object",
        "properties": {}
      },
      "desc": "给表单设置初始数据且触发表单项值变化输出"
    },
    {
      "id": "setInitialValues",
      "title": "设置表单数据",
      "schema": {
        "type": "object",
        "properties": {}
      },
      "desc": "给表单设置初始数据"
    },
    {
      "id": "submit",
      "title": "提交表单",
      "rels": [
        "onFinish"
      ],
      "schema": {
        "type": "any"
      },
      "desc": "触发表单进行校验并且提交数据"
    },
    {
      "id": "getFieldsValue",
      "title": "获取表单数据",
      "rels": [
        "returnValues"
      ],
      "schema": {
        "type": "any"
      },
      "desc": "获取表单数据（不会触发校验）"
    },
    {
      "id": "submitAndMerge",
      "title": "提交表单(合并输入项)",
      "rels": [
        "onMergeFinish"
      ],
      "schema": {
        "type": "object"
      },
      "desc": "触发表单进行校验并且提交数据，同时透传输入的数据"
    },
    {
      "id": "resetFields",
      "title": "重置表单",
      "rels": [
        "onResetFinish"
      ],
      "schema": {
        "type": "any"
      },
      "desc": "触发表单所有字段进行重置"
    },
    {
      "id": "setFormItemsProps",
      "title": "设置表单项配置",
      "schema": {
        "type": "object",
        "properties": {}
      },
      "desc": "动态配置表单项标题、提示语、必填样式等"
    }
  ],
  "outputs": [
    {
      "id": "onFinish",
      "title": "提交数据输出",
      "schema": {
        "type": "object",
        "properties": {}
      }
    },
    {
      "id": "onMergeFinish",
      "title": "提交数据输出",
      "schema": {
        "type": "object",
        "properties": {}
      }
    },
    {
      "id": "onValuesChange",
      "title": "字段值更新输出",
      "schema": {
        "type": "object",
        "properties": {
          "changedValues": {
            "type": "object",
            "properties": {}
          },
          "allValues": {
            "type": "object",
            "properties": {}
          }
        }
      }
    },
    {
      "id": "onResetFinish",
      "title": "重置完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "returnValidate",
      "title": "校验结果",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "returnValue",
      "title": "值输出",
      "schema": {
        "type": "object",
        "properties": {}
      }
    },
    {
      "id": "returnValues",
      "title": "表单数据输出",
      "schema": {
        "type": "object",
        "properties": {}
      }
    },
    {
      "id": "onClickSubmit",
      "title": "点击提交",
      "schema": {
        "type": "object",
        "properties": {}
      }
    },
    {
      "id": "onClickCancel",
      "title": "点击取消",
      "schema": {
        "type": "any"
      }
    }
  ],
  "_inputs": [
    {
      "id": "setValue",
      "title": "设置值",
      "schema": {
        "type": "object",
        "properties": {}
      }
    },
    {
      "id": "validate",
      "title": "校验",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValidate"
      ]
    },
    {
      "id": "getValue",
      "title": "获取值",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValue"
      ]
    }
  ],
  "slots": [
    {
      "id": "content",
      "title": "表单容器",
      "schema": "mybricks.normal-pc.form-container/form-item",
      "type": "scope",
      "inputs": [
        {
          "id": "setFieldsValue",
          "title": "表单数据输入",
          "schema": {
            "type": "object",
            "properties": {}
          }
        }
      ],
      "_inputs": [
        {
          "id": "validateTrigger",
          "title": "触发校验",
          "schema": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "title": "组件ID"
              }
            }
          }
        },
        {
          "id": "onChange",
          "title": "表单项值变化",
          "schema": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "title": "组件ID"
              },
              "value": {
                "type": "any",
                "title": "组件值"
              }
            }
          }
        }
      ]
    }
  ],
  "schema": [
    "mybricks.normal-pc.form-container/form-item",
    "*"
  ]
}
