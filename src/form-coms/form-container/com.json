{
  "title": "表单容器",
  "namespace": "mybricks.normal-pc.form-container",
  "author": "ZhuPengqiang",
  "author_name": "朱鹏强",
  "version": "1.4.43",
  "description": "表单容器",
  "icon": "./icon.svg",
  "preview": "https://f2.eckwai.com/kos/nlav11092/u_0k26mn.32244798eef642c8.png",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors/index.tsx",
  "upgrade": "./upgrade.ts",
  "ai": "./ai.ts",
  "target": {
    "toReact": "./toReact.ts"
  },
  "inputs": [
    {
      "id": "setInitialValues",
      "title": "设置表单数据",
      "schema": {
        "type": "object",
        "properties": {}
      },
      "desc": "给表单设置初始数据",
      "rels": [
        "setInitialValuesDone"
      ]
    },
    {
      "id": "setFieldsValue",
      "title": "设置表单数据（触发值变化）",
      "schema": {
        "type": "object",
        "properties": {}
      },
      "rels": [
        "setFieldsValueDone"
      ],
      "desc": "给表单设置初始数据且触发表单项值变化输出"
    },
    {
      "id": "submit",
      "title": "提交表单",
      "rels": [
        "onFinish",
        "onSubmitError"
      ],
      "schema": {
        "type": "any"
      },
      "desc": "触发表单进行校验并且提交数据"
    },
    {
      "id": "getFieldsValue",
      "title": "获取表单数据",
      "rels": [
        "returnValues"
      ],
      "schema": {
        "type": "any"
      },
      "desc": "获取表单数据（不会触发校验）"
    },
    {
      "id": "submitAndMerge",
      "title": "提交表单(合并输入项)",
      "rels": [
        "onMergeFinish",
        "onSubmitError"
      ],
      "schema": {
        "type": "object"
      },
      "desc": "触发表单进行校验并且提交数据，同时透传输入的数据"
    },
    {
      "id": "resetFields",
      "title": "重置表单",
      "rels": [
        "onResetFinish"
      ],
      "schema": {
        "type": "any"
      },
      "desc": "触发表单所有字段进行重置"
    },
    {
      "id": "setDisabled",
      "title": "设置禁用",
      "schema": {
        "type": "any"
      },
      "rels": [
        "setDisabledDone"
      ],
      "desc": "禁用表单项及操作项"
    },
    {
      "id": "setEnabled",
      "title": "设置启用",
      "schema": {
        "type": "any"
      },
      "rels": [
        "setEnabledDone"
      ],
      "desc": "启用表单项及操作项"
    },
    {
      "id": "isEditable",
      "title": "设置编辑/只读",
      "desc": "设置表单编辑/只读",
      "schema": {
        "type": "boolean"
      },
      "rels": [
        "isEditableDone"
      ]
    },
    {
      "id": "validateFields",
      "title": "校验表单项",
      "schema": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "rels":[
        "validateFieldsDone"
      ],
      "desc": "输入字段数组，触发指定的表单项校验"
    },
    {
      "id": "setFormItemsProps",
      "title": "设置表单项配置",
      "schema": {
        "type": "object",
        "properties": {}
      },
      "rels":[
        "setFormItemsPropsDone"
      ],
      "desc": "动态配置表单项标题、提示语、必填样式等"
    }
  ],
  "outputs": [
    {
      "id": "onFinish",
      "title": "提交数据输出",
      "schema": {
        "type": "object",
        "properties": {}
      }
    },
    {
      "id": "onSubmitError",
      "title": "校验失败输出",
      "schema": {
        "type": "object",
        "properties": {
          "name": {
            "title": "字段名",
            "type": "string"
          },
          "help": {
            "title": "校验信息",
            "type": "string"
          }
        }
      }
    },
    {
      "id": "onMergeFinish",
      "title": "提交数据输出",
      "schema": {
        "type": "object",
        "properties": {}
      }
    },
    {
      "id": "onValuesChange",
      "title": "字段值更新输出",
      "schema": {
        "type": "object",
        "properties": {
          "changedValues": {
            "type": "object",
            "properties": {}
          },
          "allValues": {
            "type": "object",
            "properties": {}
          }
        }
      }
    },
    {
      "id": "onResetFinish",
      "title": "重置完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "returnValidate",
      "title": "校验结果",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "returnValue",
      "title": "值输出",
      "schema": {
        "type": "object",
        "properties": {}
      }
    },
    {
      "id": "returnValues",
      "title": "表单数据输出",
      "schema": {
        "type": "object",
        "properties": {}
      }
    },
    {
      "id": "onClickSubmit",
      "title": "点击提交",
      "schema": {
        "type": "object",
        "properties": {}
      }
    },
    {
      "id": "onClickCancel",
      "title": "点击取消",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setInitialValuesDone",
      "title": "设置表单数据完成",
      "schema": {
        "type": "object",
        "properties": {}
      }
    },
    {
      "id": "setFieldsValueDone",
      "title": "设置表单数据（触发值变化）完成",
      "schema": {
        "type": "object",
        "properties": {}
      }
    },
    {
      "id": "setDisabledDone",
      "title": "设置禁用完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setEnabledDone",
      "title": "设置启用完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "isEditableDone",
      "title": "设置编辑/只读完成",
      "desc": "设置表单编辑/只读完成",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "validateFieldsDone",
      "title": "校验表单项完成",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "setFormItemsPropsDone",
      "title": "设置表单项配置完成",
      "schema": {
        "type": "object",
        "properties": {}
      }
    }
  ],
  "_inputs": [
    {
      "id": "setValue",
      "title": "设置值",
      "schema": {
        "type": "object",
        "properties": {}
      }
    },
    {
      "id": "validate",
      "title": "校验",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValidate"
      ]
    },
    {
      "id": "getValue",
      "title": "获取值",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValue"
      ]
    }
  ],
  "slots": [
    {
      "id": "content",
      "title": "表单容器",
      "schema": [
        "mybricks.normal-pc.form-container/form-item",
        "mybricks.normal-pc.form-container/form-addition-container"
      ],
      "type": "scope",
      "inputs": [
        {
          "id": "setFieldsValue",
          "title": "表单数据输入",
          "schema": {
            "type": "object",
            "properties": {}
          }
        }
      ],
      "_inputs": [
        {
          "id": "validateTrigger",
          "title": "触发校验",
          "schema": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "title": "组件ID"
              }
            }
          }
        },
        {
          "id": "onChange",
          "title": "表单项值变化",
          "schema": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "title": "组件ID"
              },
              "value": {
                "type": "any",
                "title": "组件值"
              }
            }
          }
        }
      ]
    }
  ],
  "schema": [
    "mybricks.domain-pc.crud/query",
    "*"
  ]
}