{
  "title": "下拉框",
  "namespace": "mybricks.normal-pc.select",
  "version": "1.0.11",
  "description": "下拉框",
  "author": "ZhuLin",
  "author_name": "朱琳",
  "icon": "./icon.svg",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.ts",
  "upgrade": "./upgrade.ts",
  "target": {
    "toReact": "./toReact.ts"
  },
  "inputs": [
    {
      "id": "setValue",
      "title": "设置值",
      "desc": "设置下拉框的值，并触发值变化事件输出",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setInitialValue",
      "title": "设置初始值",
      "desc": "设置下拉框的初始值，并触发初始化事件输出",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "validate",
      "title": "校验",
      "desc": "触发下拉框的校验检查，并输出校验结果",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValidate"
      ]
    },
    {
      "id": "getValue",
      "title": "获取值",
      "desc": "获取下拉框的值并输出",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValue"
      ]
    },
    {
      "id": "resetValue",
      "title": "重置值",
      "desc": "清空下拉框的值",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setOptions",
      "title": "设置数据源",
      "schema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "label": {
              "title": "标签",
              "type": "string"
            },
            "value": {
              "title": "值",
              "type": "string"
            },
            "disabled": {
              "title": "禁用",
              "type": "boolean"
            },
            "checked": {
              "title": "选中",
              "type": "boolean"
            }
          }
        }
      }
    },
    {
      "id": "setDisabled",
      "title": "设置禁用",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setEnabled",
      "title": "设置启用",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setLoading",
      "title": "设置加载中状态",
      "schema": {
        "type": "boolean"
      }
    }
  ],
  "outputs": [
    {
      "id": "onInitial",
      "title": "初始化",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onChange",
      "title": "值更新",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onBlur",
      "title": "失去焦点",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "returnValidate",
      "title": "校验结果",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "title": "校验状态",
            "type": "string"
          },
          "help": {
            "title": "校验提示",
            "type": "string"
          }
        }
      }
    },
    {
      "id": "returnValue",
      "title": "值输出",
      "schema": {
        "type": "string"
      }
    }
  ],
  "schema": [
    "mybricks.normal-pc.form-container/form-item",
    "*"
  ]
}