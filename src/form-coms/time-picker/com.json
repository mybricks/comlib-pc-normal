{
  "title": "时间选择框",
  "namespace": "mybricks.normal-pc.time-picker",
  "author": "tangxiaoxin",
  "author_name": "唐小新",
  "version": "1.0.4",
  "description": "时间选择框",
  "docs": "组件使用说明文档链接",
  "icon": "./icon.svg",
  "runtime": "./runtime.tsx",
  "data": "./data.json",
  "editors": "./editors.ts",
  "upgrade": "./upgrade.ts",
  "target": {
    "toReact": "./toReact.ts"
  },
  "inputs": [
    {
      "id": "setValue",
      "title": "设置值",
      "schema": {
        "type": "number"
      },
      "desc": "设置时间"
    },
    {
      "id": "setInitialValue",
      "title": "设置初始值",
      "schema": {
        "type": "number"
      },
      "desc": "设置时间选择框初始值"
    },
    {
      "id": "getValue",
      "title": "获取值",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValue"
      ],
      "desc": "获取时间"
    },
    {
      "id": "resetValue",
      "title": "重置值",
      "schema": {
        "type": "any"
      },
      "desc": "重置时间"
    },
    {
      "id": "validate",
      "title": "校验",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValidate"
      ]
    },
    {
      "id": "setDisabled",
      "title": "设置禁用",
      "schema": {
        "type": "any"
      },
      "desc": "禁用时间选择框"
    },
    {
      "id": "setEnabled",
      "title": "设置启用",
      "schema": {
        "type": "any"
      },
      "desc": "启用时间选择框"
    }
  ],
  "outputs": [
    {
      "id": "onInitial",
      "title": "初始化",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "returnValue",
      "title": "数据输出",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "returnValidate",
      "title": "校验结果",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "title": "校验状态",
            "type": "string"
          },
          "help": {
            "title": "校验提示",
            "type": "string"
          }
        }
      }
    },
    {
      "id": "onChange",
      "title": "值更新",
      "schema": {
        "type": "number"
      }
    }
  ],
  "slots": [],
  "schema": [
    "mybricks.normal-pc.form-container/form-item",
    "*"
  ]
}