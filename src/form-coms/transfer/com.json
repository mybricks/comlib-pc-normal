{
  "title": "穿梭框",
  "namespace": "mybricks.normal-pc.transfer",
  "author": "tangxiaoxin",
  "author_name": "tangxiaoxin",
  "version": "1.1.14",
  "description": "组件描述",
  "docs": "组件使用说明文档链接",
  "icon": "./icon.svg",
  "preview": "https://f2.eckwai.com/kos/nlav11092/u_rdyy4i.0a226a8a9030181e.png",
  "runtime": "./runtime.tsx",
  "data": "./data.json",
  "editors": "./editors.tsx",
  "upgrade": "./upgrade.ts",
  "target": {
    "toReact": "./toReact.ts"
  },
  "inputs": [
    {
      "id": "setSource",
      "title": "设置数据源",
      "schema": {
        "type": "array",
        "description": "数组格式的数据源，渲染到左侧,需要包含以下字段title, key，description等字段",
        "items": {
          "type": "object",
          "properties": {
            "key": {
              "type": "string"
            },
            "title": {
              "type": "string"
            },
            "description": {
              "type": "string"
            },
            "disabled": {
              "type": "boolean",
              "description": "是否禁用，可选的"
            }
          }
        }
      },
      "desc": "给穿梭框设置数据源",
      "rels": [
        "setSourceDone"
      ]
    },
    {
      "id": "setValue",
      "title": "设置值",
      "schema": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "desc": "给穿梭框设置目标值",
      "rels": [
        "setValueDone"
      ]
    },
    {
      "id": "setInitialValue",
      "title": "设置初始值",
      "desc": "设置穿梭框始值，并触发值初始化事件输出",
      "schema": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "rels": [
        "setInitialValueDone"
      ]
    },
    {
      "id": "getValue",
      "title": "获取值",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValue"
      ],
      "desc": "获取穿梭框目标值"
    },
    {
      "id": "resetValue",
      "title": "重置值",
      "schema": {
        "type": "any"
      },
      "desc": "重置穿梭框目标值",
      "rels": [
        "resetValueDone"
      ]
    },
    {
      "id": "validate",
      "title": "校验",
      "desc": "触发穿梭框的校验检查，并输出校验结果",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValidate"
      ]
    },
    {
      "id": "setDisabled",
      "title": "设置禁用",
      "schema": {
        "type": "any"
      },
      "desc": "禁用穿梭框组件,并触发启用完成事件输出",
      "rels": [
        "setDisabledDone"
      ]
    },
    {
      "id": "setEnabled",
      "title": "设置启用",
      "schema": {
        "type": "any"
      },
      "desc": "启用穿梭框组件,并触发启用完成事件输出",
      "rels": [
        "setEnabledDone"
      ]
    },
    {
      "id": "isEnable",
      "title": "启用/禁用",
      "desc": "对开关进行启用/禁用状态设置，并触发启用/禁用完成事件",
      "schema": {
        "type": "boolean"
      },
      "rels": [
        "isEnableDone"
      ]
    },
    {
      "id": "setValidateInfo",
      "title": "设置校验状态",
      "desc": "设置表单项的校验状态和校验失败提示文案",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "description": "校验状态，成功/失败",
            "type": "enum",
            "items": [
              {
                "type": "string",
                "value": "success"
              },
              {
                "type": "string",
                "value": "error"
              }
            ]
          },
          "help": {
            "description": "帮助提示信息",
            "type": "string"
          }
        }
      },
      "rels": [
        "setValidateInfoDone"
      ]
    }
  ],
  "outputs": [
    {
      "id": "onInitial",
      "title": "值初始化",
      "desc": "穿梭框的值初始化事件输出",
      "schema": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    {
      "id": "returnValue",
      "title": "数据输出",
      "desc": "获取穿梭框的值后，值输出",
      "schema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "key": {
              "type": "string"
            },
            "title": {
              "type": "string"
            },
            "description": {
              "type": "string"
            },
            "disabled": {
              "type": "boolean"
            }
          }
        }
      }
    },
    {
      "id": "onChange",
      "title": "目标数据更新",
      "desc": "穿梭框的目标数据更新事件输出",
      "schema": {
        "type": "array",
        "description": "数组格式的目标数据源，选中的数据列表，包含基础key,title,description等字段",
        "items": {
          "type": "object",
          "properties": {
            "key": {
              "type": "string"
            },
            "title": {
              "type": "string"
            },
            "description": {
              "type": "string"
            },
            "disabled": {
              "type": "boolean"
            }
          }
        }
      }
    },
    {
      "id": "onValidate",
      "title": "校验触发",
      "desc": "穿梭框的校验触发事件输出",
      "schema": {
        "title": "当前值",
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    {
      "id": "returnValidate",
      "title": "校验结果",
      "desc": "穿梭框输出校验结果",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "title": "校验状态",
            "type": "string",
            "description": "校验状态，成功/失败"
          },
          "help": {
            "title": "校验提示",
            "type": "string",
            "description": "帮助提示信息"
          }
        }
      }
    },
    {
      "id": "setSourceDone",
      "title": "设置数据源完成",
      "schema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "key": {
              "type": "string"
            },
            "title": {
              "type": "string"
            },
            "description": {
              "type": "string"
            },
            "disabled": {
              "type": "boolean"
            }
          }
        }
      }
    },
    {
      "id": "setValueDone",
      "title": "设置值完成",
      "schema": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    {
      "id": "setInitialValueDone",
      "title": "设置初始值完成",
      "schema": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    {
      "id": "resetValueDone",
      "title": "重置完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setDisabledDone",
      "title": "禁用完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setEnabledDone",
      "title": "启用完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "isEnableDone",
      "title": "启用/禁用完成",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "setValidateInfoDone",
      "title": "设置校验状态完成",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "description": "校验状态，成功/失败",
            "type": "enum",
            "items": [
              {
                "type": "string",
                "value": "success"
              },
              {
                "type": "string",
                "value": "error"
              }
            ]
          },
          "help": {
            "description": "帮助提示信息",
            "type": "string"
          }
        }
      }
    }
  ],
  "slots": [],
  "schema": [
    "mybricks.normal-pc.form-container/form-item",
    "*"
  ]
}