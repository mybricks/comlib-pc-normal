{
  "title": "自动完成",
  "namespace": "mybricks.normal-pc.auto-complete",
  "author": "HuangQiuyun",
  "author_name": "黄秋云",
  "version": "1.1.6",
  "description": "自动完成是一个带提示的文本输入框, 用户可以自由输入, 关键词是辅助输入",
  "icon": "./icon.svg",
  "preview": "https://f2.eckwai.com/kos/nlav11092/u_0fcijz.b5f0933284f32e40.png",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.ts",
  "upgrade": "./upgrade.ts",
  "target": {
    "toReact": "./toReact.ts"
  },
  "inputs": [
    {
      "id": "setValue",
      "title": "设置值",
      "desc": "设置自动完成的值，并触发值变化事件输出",
      "schema": {
        "type": "string"
      },
      "rels": [
        "setValueDone"
      ]
    },
    {
      "id": "setInitialValue",
      "title": "设置初始值",
      "desc": "设置自动完成的初始值，并触发初始化事件输出",
      "schema": {
        "type": "string"
      },
      "rels":[
        "setInitialValueDone"
      ]
    },
    {
      "id": "validate",
      "title": "校验",
      "desc": "触发自动完成的校验检查，并输出校验结果",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValidate"
      ]
    },
    {
      "id": "getValue",
      "title": "获取值",
      "desc": "获取自动完成的值并输出",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValue"
      ]
    },
    {
      "id": "resetValue",
      "title": "重置值",
      "desc": "清空自动完成的值",
      "schema": {
        "type": "any"
      },
      "rels":[
        "resetValueDone"
      ]
    },
    {
      "id": "setDisabled",
      "title": "设置禁用",
      "schema": {
        "type": "any"
      },
      "rels": [
        "setDisabledDone"
      ]
    },
    {
      "id": "setEnabled",
      "title": "设置启用",
      "schema": {
        "type": "any"
      },
      "rels": [
        "setEnabledDone"
      ]
    },
    {
      "id": "isEnable",
      "title": "启用/禁用",
      "schema": {
        "type": "boolean"
      },
      "rels": [
        "isEnableDone"
      ]
    },
    {
      "id": "setColor",
      "title": "设置字体颜色",
      "desc": "设置输入框字体颜色",
      "schema": {
        "type": "string"
      },
      "rels": [
        "setColorDone"
      ]
    },
    {
      "id": "setOptions",
      "title": "输入数据源",
      "desc": "设置自动完成的下拉选项列表",
      "schema": {
        "title": "输入数据源数据",
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "label": {
              "title": "标签",
              "type": "string"
            },
            "value": {
              "title": "值",
              "type": "string"
            }
          }
        }
      },
      "rels": [
        "setOptionsDone"
      ]
    },
    {
      "id": "setValidateInfo",
      "title": "设置校验状态",
      "desc": "设置表单项的校验状态和校验失败提示文案",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "type": "enum",
            "items": [
              {
                "type": "string",
                "value": "success"
              },
              {
                "type": "string",
                "value": "error"
              }
            ]
          },
          "help": {
            "type": "string"
          }
        }
      },
      "rels": [
        "setValidateInfoDone"
      ]
    }
  ],
  "outputs": [
    {
      "id": "onInitial",
      "title": "值初始化",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onChange",
      "title": "值更新",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onBlur",
      "title": "失去焦点",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onSelect",
      "title": "值选择",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "onValidate",
      "title": "校验触发",
      "schema": {
        "title": "当前值",
        "type": "string"
      }
    },
    {
      "id": "returnValidate",
      "title": "校验结果",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "title": "校验状态",
            "type": "string"
          },
          "help": {
            "title": "校验提示",
            "type": "string"
          }
        }
      }
    },
    {
      "id": "returnValue",
      "title": "值输出",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setValueDone",
      "title": "设置值完成",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setInitialValueDone",
      "title": "设置初始值完成",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "resetValueDone",
      "title": "重置完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setDisabledDone",
      "title": "禁用完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setEnabledDone",
      "title": "启用完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "isEnableDone",
      "title": "启用/禁用完成",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "setColorDone",
      "title": "设置字体颜色完成",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setOptionsDone",
      "title": "设置数据源完成",
      "schema": {
        "title": "输入数据源数据",
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "label": {
              "title": "标签",
              "type": "string"
            },
            "value": {
              "title": "值",
              "type": "string"
            }
          }
        }
      }
    },
    {
      "id": "setValidateInfoDone",
      "title": "设置校验状态完成",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "type": "enum",
            "items": [
              {
                "type": "string",
                "value": "success"
              },
              {
                "type": "string",
                "value": "error"
              }
            ]
          },
          "help": {
            "type": "string"
          }
        }
      }
    }
  ],
  "schema": [
    "mybricks.normal-pc.form-container/form-item",
    "*"
  ]
}