{
  "title": "单选框",
  "namespace": "mybricks.normal-pc.radio",
  "version": "1.1.17",
  "description": "单选框",
  "author": "ZhuLin",
  "author_name": "朱琳",
  "icon": "./icon.svg",
  "preview": "https://f2.eckwai.com/kos/nlav11092/u_9en9i2.6e0f524f237f3fa5.png",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.ts",
  "upgrade": "./upgrade.ts",
  "target": {
    "toReact": "./toReact.ts"
  },
  "inputs": [
    {
      "id": "setValue",
      "title": "设置值",
      "desc": "设置单选框的值，并触发值变化事件输出",
      "schema": {
        "type": "string"
      },
      "rels": ["setValueDone"]
    },
    {
      "id": "setInitialValue",
      "title": "设置初始值",
      "desc": "设置单选框的初始值，并触发值初始化事件输出",
      "schema": {
        "type": "string"
      },
      "rels": ["setInitialValueDone"]
    },
    {
      "id": "validate",
      "title": "校验",
      "desc": "触发单选框的校验检查，并输出校验结果",
      "schema": {
        "type": "any"
      },
      "rels": ["returnValidate"]
    },
    {
      "id": "getValue",
      "title": "获取值",
      "desc": "获取单选框的值并输出",
      "schema": {
        "type": "any"
      },
      "rels": ["returnValue"]
    },
    {
      "id": "resetValue",
      "title": "重置值",
      "desc": "清空单选框的值",
      "schema": {
        "type": "any"
      },
      "rels": ["resetValueDone"]
    },
    {
      "id": "setOptions",
      "title": "设置数据源",
      "desc": "设置单选框的选项列表",
      "schema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "label": {
              "title": "标签",
              "type": "string",
              "description": "标签"
            },
            "value": {
              "title": "值",
              "type": "any",
              "description": "值"
            },
            "disabled": {
              "title": "禁用",
              "type": "boolean",
              "description": "是否禁用"
            },
            "checked": {
              "title": "选中",
              "type": "boolean",
              "description": "是否选中"
            }
          }
        }
      },
      "rels": ["setOptionsDone"]
    },
    {
      "id": "setDisabled",
      "title": "设置禁用",
      "desc": "禁用单选框，阻止用户选择",
      "schema": {
        "type": "any"
      },
      "rels": ["setDisabledDone"]
    },
    {
      "id": "setEnabled",
      "title": "设置启用",
      "desc": "启用单选框，允许用户选择",
      "schema": {
        "type": "any"
      },
      "rels": ["setEnabledDone"]
    },
    {
      "id": "isEnable",
      "title": "启用/禁用",
      "desc": "根据输入值控制单选框启用/禁用，true-启用，false-禁用",
      "schema": {
        "type": "boolean"
      },
      "rels": ["isEnableDone"]
    },
    {
      "id": "isEditable",
      "title": "设置编辑/只读",
      "desc": "设置表单项编辑/只读态",
      "schema": {
        "type": "boolean"
      },
      "rels": ["isEditableDone"]
    },
    {
      "id": "setActiveFontColor",
      "title": "设置激活选项字体的颜色",
      "desc": "设置激活选项字体的颜色",
      "schema": {
        "type": "string"
      },
      "rels": ["setActiveFontColorDone"]
    },
    {
      "id": "setValidateInfo",
      "title": "设置校验状态",
      "desc": "设置表单项的校验状态和校验失败提示文案",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "type": "enum",
            "description": "校验状态",
            "items": [
              {
                "type": "string",
                "value": "success"
              },
              {
                "type": "string",
                "value": "error"
              }
            ]
          },
          "help": {
            "type": "string",
            "description": "校验提示"
          }
        }
      },
      "rels": ["setValidateInfoDone"]
    }
  ],
  "outputs": [
    {
      "id": "onInitial",
      "title": "值初始化",
      "desc": "单选框值初始化时触发，输出单选框初始值",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onChange",
      "title": "值更新",
      "desc": "单选框值更新时触发，输出单选框当前值",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onValidate",
      "title": "校验触发",
      "desc": "单选框校验时触发，输出单选框当前值",
      "schema": {
        "title": "当前值",
        "type": "string"
      }
    },
    {
      "id": "returnValidate",
      "title": "校验结果",
      "desc": "输出单选框校验结果",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "title": "校验状态",
            "type": "string",
            "description": "校验状态"
          },
          "help": {
            "title": "校验提示",
            "type": "string",
            "description": "校验提示"
          }
        }
      }
    },
    {
      "id": "returnValue",
      "title": "值输出",
      "desc": "输出单选框当前值",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setValueDone",
      "title": "设置值完成",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setInitialValueDone",
      "title": "设置初始值完成",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "resetValueDone",
      "title": "重置完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setOptionsDone",
      "title": "设置数据源完成",
      "schema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "label": {
              "title": "标签",
              "type": "string",
              "description": "标签"
            },
            "value": {
              "title": "值",
              "type": "any",
              "description": "值"
            },
            "disabled": {
              "title": "禁用",
              "type": "boolean",
              "description": "是否禁用"
            },
            "checked": {
              "title": "选中",
              "type": "boolean",
              "description": "是否选中"
            }
          }
        }
      }
    },
    {
      "id": "setDisabledDone",
      "title": "禁用完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setEnabledDone",
      "title": "启用完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "isEnableDone",
      "title": "启用/禁用完成",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "isEditableDone",
      "title": "设置编辑/只读完成",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "setActiveFontColorDone",
      "title": "设置激活选项字体的颜色完成",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setValidateInfoDone",
      "title": "设置校验状态完成",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "type": "enum",
            "description": "校验状态",
            "items": [
              {
                "type": "string",
                "value": "success"
              },
              {
                "type": "string",
                "value": "error"
              }
            ]
          },
          "help": {
            "type": "string",
            "description": "校验提示"
          }
        }
      }
    }
  ],
  "schema": ["mybricks.normal-pc.form-container/form-item", "*"]
}
