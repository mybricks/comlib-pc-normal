{
  "title": "代码编辑器",
  "namespace": "mybricks.normal-pc.code-editor",
  "author": "tangxiaoxin",
  "author_name": "唐小新",
  "version": "1.1.10",
  "description": "用于在线编辑代码，支持多种编程语言",
  "runtime": "./runtime.tsx",
  "editors": "./editor.ts",
  "data": "./data.json",
  "icon": "./icon.svg",
  "upgrade": "./upgrade.ts",
  "preview": "https://f2.eckwai.com/kos/nlav12333/fangzhou/pub/compress/image-9e7987dd-27c4-4375-8c8c-82465588a0dd.png",
  "inputs": [
    {
      "id": "setValue",
      "title": "设置值",
      "schema": {
        "type": "string"
      },
      "rels": [
        "setValueDone"
      ],
      "desc": "设置代码"
    },
    {
      "id": "setInitialValue",
      "title": "设置初始值",
      "schema": {
        "type": "string"
      },
      "rels":[
        "setInitialValueDone"
      ],
      "desc": "设置代码框初始值"
    },
    {
      "id": "getValue",
      "title": "获取值",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValue"
      ],
      "desc": "获取代码"
    },
    {
      "id": "resetValue",
      "title": "重置值",
      "schema": {
        "type": "any"
      },
      "rels":[
        "resetValueDone"
      ],
      "desc": "重置"
    },
    {
      "id": "validate",
      "title": "校验",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValidate"
      ]
    },
    {
      "id": "setDisabled",
      "title": "设置禁用",
      "schema": {
        "type": "any"
      },
      "rels": [
        "setDisabledDone"
      ],
      "desc": "代码不可编辑"
    },
    {
      "id": "setEnabled",
      "title": "设置启用",
      "schema": {
        "type": "any"
      },
      "rels": [
        "setEnabledDone"
      ],
      "desc": "代码可编辑"
    },
    {
      "id": "isEnable",
      "title": "启用/禁用",
      "schema": {
        "type": "boolean"
      },
      "rels": [
        "isEnableDone"
      ]
    },
    {
      "id": "setValidateInfo",
      "title": "设置校验状态",
      "desc": "设置表单项的校验状态和校验失败提示文案",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "type": "enum",
            "items": [
              {
                "type": "string",
                "value": "success"
              },
              {
                "type": "string",
                "value": "error"
              }
            ]
          },
          "help": {
            "type": "string"
          }
        }
      },
      "rels": [
        "setValidateInfoDone"
      ]
    }
  ],
  "outputs": [
    {
      "id": "onInitial",
      "title": "值初始化",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onValidate",
      "title": "校验触发",
      "schema": {
        "title": "当前值",
        "type": "string"
      }
    },
    {
      "id": "returnValue",
      "title": "数据输出",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "returnValidate",
      "title": "校验结果",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "title": "校验状态",
            "type": "string"
          },
          "help": {
            "title": "校验提示",
            "type": "string"
          }
        }
      }
    },
    {
      "id": "onChange",
      "title": "值更新",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setValueDone",
      "title": "设置值完成",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setInitialValueDone",
      "title": "设置初始值完成",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "resetValueDone",
      "title": "重置完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setDisabledDone",
      "title": "禁用完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setEnabledDone",
      "title": "启用完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "isEnableDone",
      "title": "启用/禁用完成",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "setValidateInfoDone",
      "title": "设置校验状态完成",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "type": "enum",
            "items": [
              {
                "type": "string",
                "value": "success"
              },
              {
                "type": "string",
                "value": "error"
              }
            ]
          },
          "help": {
            "type": "string"
          }
        }
      }
    }
  ],
  "schema": [
    "mybricks.normal-pc.form-container/form-item",
    "*"
  ]
}