{
  "title": "滑动输入条",
  "namespace": "mybricks.normal-pc.slider",
  "version": "1.0.7",
  "description": "滑动输入条",
  "author": "ZhuLin",
  "author_name": "朱琳",
  "icon": "./icon.svg",
  "preview": "https://f2.eckwai.com/kos/nlav11092/u_37f9sr.72675c597ee8dcc7.png",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.ts",
  "upgrade": "./upgrade.ts",
  "target": {
    "toReact": "./toReact.ts"
  },
  "inputs": [
    {
      "id": "setValue",
      "title": "设置值",
      "desc": "设置滑动输入条的值，并触发值变化事件输出",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "setInitialValue",
      "title": "设置初始值",
      "desc": "设置滑动输入条的初始值，并触发值初始化事件输出",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "validate",
      "title": "校验",
      "desc": "触发滑动输入条的校验检查，并输出校验结果",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValidate"
      ]
    },
    {
      "id": "getValue",
      "title": "获取值",
      "desc": "获取滑动输入条的值并输出",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValue"
      ]
    },
    {
      "id": "resetValue",
      "title": "重置值",
      "desc": "清空滑动输入条的值",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setDisabled",
      "title": "设置禁用",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setEnabled",
      "title": "设置启用",
      "schema": {
        "type": "any"
      }
    }
  ],
  "outputs": [
    {
      "id": "onInitial",
      "title": "值初始化",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "onChange",
      "title": "值更新",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "returnValidate",
      "title": "校验结果",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "title": "校验状态",
            "type": "string"
          },
          "help": {
            "title": "校验提示",
            "type": "string"
          }
        }
      }
    },
    {
      "id": "returnValue",
      "title": "值输出",
      "schema": {
        "type": "number"
      }
    }
  ],
  "schema": [
    "mybricks.normal-pc.form-container/form-item",
    "*"
  ]
}