{
  "title": "日期选择框",
  "namespace": "mybricks.normal-pc.date-picker",
  "author": "HuangQiuyun",
  "author_name": "黄秋云",
  "version": "1.1.41",
  "description": "日期选择框",
  "icon": "./icon.svg",
  "preview": "https://f2.eckwai.com/kos/nlav11092/u_taeb6n.f449835eb18b532c.png",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.ts",
  "upgrade": "./upgrade.ts",
  "ai": "./ai.ts",
  "target": {
    "toReact": "./toReact.ts"
  },
  "inputs": [
    {
      "id": "setValue",
      "title": "设置值",
      "desc": "设置日期选择的值，并触发值变化事件输出",
      "schema": {
        "type": "string"
      },
      "rels": [
        "setValueDone"
      ]
    },
    {
      "id": "setInitialValue",
      "title": "设置初始值",
      "desc": "设置日期选择的初始值，并触发初始化事件输出",
      "schema": {
        "type": "string"
      },
      "rels": [
        "setInitialValueDone"
      ]
    },
    {
      "id": "validate",
      "title": "校验",
      "desc": "触发日期选择的校验检查，并输出校验结果",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValidate"
      ]
    },
    {
      "id": "getValue",
      "title": "获取值",
      "desc": "获取日期选择的值并输出",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValue"
      ]
    },
    {
      "id": "resetValue",
      "title": "重置值",
      "desc": "清空日期选择的值",
      "schema": {
        "type": "any"
      },
      "rels": [
        "resetValueDone"
      ]
    },
    {
      "id": "setDisabled",
      "title": "设置禁用",
      "desc": "禁用日期选择框，阻止用户选择日期",
      "schema": {
        "type": "any"
      },
      "rels": [
        "setDisabledDone"
      ]
    },
    {
      "id": "setEnabled",
      "title": "设置启用",
      "desc": "启用日期选择框，允许用户选择日期",
      "schema": {
        "type": "any"
      },
      "rels": [
        "setEnabledDone"
      ]
    },
    {
      "id": "isEnable",
      "title": "启用/禁用",
      "desc": "根据输入值控制日期选择框启用/禁用，true-启用，false-禁用",
      "schema": {
        "type": "boolean"
      },
      "rels": [
        "isEnableDone"
      ]
    },
    {
      "id": "isEditable",
      "title": "设置编辑/只读",
      "desc": "设置表单项编辑/只读态",
      "schema": {
        "type": "boolean"
      },
      "rels": [
        "isEditableDone"
      ]
    },
    {
      "id": "setValidateInfo",
      "title": "设置校验结果",
      "desc": "配合自定义校验使用，设置表单项的校验状态和校验失败提示文案",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "type": "enum",
            "description": "校验状态",
            "items": [
              {
                "type": "string",
                "value": "success"
              },
              {
                "type": "string",
                "value": "error"
              }
            ]
          },
          "help": {
            "type": "string",
            "description": "校验提示"
          }
        }
      },
      "rels": [
        "setValidateInfoDone"
      ]
    },
    {
      "id": "setDisabledDateRules",
      "title": "设置禁用特定日期规则",
      "desc": "设置不可选择的日期规则",
      "schema": {
        "type": "object",
        "properties": {
          "rules": {
            "type": "array",
            "descriptions": "禁用规则",
            "items": {
              "type": "object",
              "descriptions": "单条规则，数组内容，第一项为操作关系符号，如",
              "properties": {
                "value": {
                  "title": "比较值",
                  "description": "可以是字符或者数组，例如 `TODAY`、`2024-07-17` 可用在>=; ['2024-07-16', '2024-07-28']可用在in、between；'Sat'、'Mon',  用在equal场景",
                  "type": "any"
                },
                "op":  {
                  "type": "enum",
                  "title": "比较符号",
                  "description": "比较操作，例如，>、>=、<= 具体日期，以及in、notIn区间包含关系；equal、notEqual特定日期（周六日、季度一二）；between 日期区间；complex 用于复杂场景，value可取{relation: '&&', rules: []}",
                  "items": [
                    {
                      "type": "string",
                      "value": ">"
                    },
                    {
                      "type": "string",
                      "value": ">="
                    },
                    {
                      "type": "string",
                      "value": "<"
                    },
                    {
                      "type": "string",
                      "value": "<="
                    },
                    {
                      "type": "string",
                      "value": "="
                    },
                    {
                      "type": "string",
                      "value": "!="
                    },
                    {
                      "type": "string",
                      "value": "in"
                    },
                    {
                      "type": "string",
                      "value": "notIn"
                    },
                    {
                      "type": "string",
                      "value": "equal"
                    },
                    {
                      "type": "string",
                      "value": "notEqual"
                    },
                    {
                      "type": "string",
                      "value": "between"
                    }, 
                    {
                      "type": "string",
                      "value": "complex"
                    }
                  ]
                }                   
              }
            } 
          },
          "relation": {
            "type": "enum",
            "descriptions": "禁用规则中, 多项规则间关系；支持 && 和 || ，不传时默认为||",
            "items": [
              {
                "type": "string",
                "value": "&&"
              },
              {
                "type": "string",
                "value": "||"
              }
            ]
          },
          "picker": {
            "type": "enum",
            "description": "处理的日期选择类型，非必填，不传时以组件配置的类型为准",
            "items": [
              {
                "type": "string",
                "value": "date"
              },
              {
                "type": "string",
                "value": "week"
              },
              {
                "type": "string",
                "value": "month"
              },
              {
                "type": "string",
                "value": "quarter"
              },
              {
                "type": "string",
                "value": "year"
              }
            ]
          },
          "currentMode": {
            "type": "enum",
            "descriptions": "当前时间比较方式，不传递，则是默认；传递startOf、endOf分别表示日期开始时间、日期结束时间",
            "items": [
              {
                "type": "string",
                "value": "default"
              },
              {
                "type": "string",
                "value": "startOf"
              },
              {
                "type": "string",
                "value": "endOf"
              }
            ]
          }
        }
      },
      "rels": [
        "setDisabledDateRulesDone"
      ]
    },
    {
      "id": "setColor",
      "title": "设置字体颜色",
      "desc": "设置日期选择框里字体的颜色",
      "schema": {
        "type": "string"
      },
      "rels": [
        "setColorDone"
      ]
    },
    {
      "id": "setDateType",
      "title": "设置日期选择类型",
      "desc": "设置日期选择类型",
      "schema": {
        "type": "enum",
        "items": [
          {
            "type": "string",
            "value": "date"
          },
          {
            "type": "string",
            "value": "week"
          },
          {
            "type": "string",
            "value": "month"
          },
          {
            "type": "string",
            "value": "quarter"
          },
          {
            "type": "string",
            "value": "year"
          }
        ]
      },
      "rels": [
        "setDateTypeDone"
      ]
    }
  ],
  "outputs": [
    {
      "id": "onInitial",
      "title": "值初始化",
      "desc": "日期选择框值初始化时触发，输出日期选择框初始值",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onChange",
      "title": "值更新",
      "desc": "日期选择框值更新时触发，输出日期选择框当前值",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onValidate",
      "title": "校验触发",
      "desc": "日期选择框校验时触发，输出日期选择框当前值",
      "schema": {
        "title": "当前值",
        "type": "string"
      }
    },
    {
      "id": "returnValidate",
      "title": "校验结果",
      "desc": "输出日期选择框校验结果",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "type": "string",
            "description": "校验状态"
          },
          "help": {
            "type": "string",
            "description": "校验提示"
          }
        }
      }
    },
    {
      "id": "returnValue",
      "title": "值输出",
      "desc": "输出日期选择框当前值",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setValueDone",
      "title": "设置值完成",
      "desc": "设置初始值事件完成，输出当前设置的值",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setInitialValueDone",
      "title": "设置初始值完成",
      "desc": "设置初始值事件完成，输出设置的初始值",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "resetValueDone",
      "title": "重置完成",
      "desc": "重置值事件完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setDisabledDone",
      "title": "禁用完成",
      "desc": "禁用完成事件完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setEnabledDone",
      "title": "启用完成",
      "desc": "启用完成事件完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "isEnableDone",
      "title": "启用/禁用完成",
      "desc": "启用/禁用事件完成",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "isEditableDone",
      "title": "设置编辑/只读完成",
      "desc": "设置编辑/只读事件完成",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "setValidateInfoDone",
      "title": "设置校验状态完成",
      "desc": "设置校验状态事件完成",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "type": "enum",
            "description": "校验状态",
            "items": [
              {
                "type": "string",
                "value": "success"
              },
              {
                "type": "string",
                "value": "error"
              }
            ]
          },
          "help": {
            "type": "string",
            "description": "校验提示"
          }
        }
      }
    },
    {
      "id": "setDisabledDateRulesDone",
      "title": "禁用特定日期规则完成",
      "desc": "禁用特定日期事件完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setColorDone",
      "title": "设置字体颜色完成",
      "desc": "设置字体颜色事件完成，输入字体颜色",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setDateTypeDone",
      "title": "设置日期选择类型完成",
      "desc": "设置日期选择类型事件完成，输出日期选择类型",
      "schema": {
        "type": "enum",
        "items": [
          {
            "type": "string",
            "value": "date"
          },
          {
            "type": "string",
            "value": "week"
          },
          {
            "type": "string",
            "value": "month"
          },
          {
            "type": "string",
            "value": "quarter"
          },
          {
            "type": "string",
            "value": "year"
          }
        ]
      }
    },
    {
      "id": "onPanelChange",
      "title": "日期面板切换",
      "desc": "日期面板切换时触发，输出日期选择框当前值和日期选择类型",
      "schema": {
        "type": "object",
        "properties": {
          "value": {
            "title": "日期",
            "type": "string",
            "description": "日期"
          },
          "mode": {
            "title": "日期选择类型",
            "type": "string",
            "description": "日期选择类型"
          }
        }
      }
    }
  ],
  "schema": [
    "mybricks.normal-pc.form-container/form-item",
    "*"
  ]
}