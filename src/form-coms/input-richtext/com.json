{
  "title": "富文本输入框",
  "namespace": "mybricks.normal-pc.input-richtext",
  "version": "1.1.23",
  "description": "富文本输入框",
  "author": "HuangQiuyun",
  "author_name": "黄秋云",
  "icon": "./icon.svg",
  "preview": "https://f2.eckwai.com/kos/nlav11092/u_3dxwcb.fc6aad3a9dd9faf3.png",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.tsx",
  "upgrade": "./upgrade.ts",
  "inputs": [
    {
      "id": "setValue",
      "title": "设置值",
      "desc": "设置富文本输入框的值，并触发值变化事件输出",
      "schema": {
        "type": "string"
      },
      "rels": [
        "setValueDone"
      ]
    },
    {
      "id": "setInitialValue",
      "title": "设置初始值",
      "desc": "设置富文本输入框的初始值，并触发初始化事件输出",
      "schema": {
        "type": "string"
      },
      "rels":[
        "setInitialValueDone"
      ]
    },
    {
      "id": "validate",
      "title": "校验",
      "desc": "触发富文本输入框的校验检查，并输出校验结果",
      "schema": {
        "type": "any"
      },
      "rels": ["returnValidate"]
    },
    {
      "id": "getValue",
      "title": "获取值",
      "desc": "获取富文本输入框的值并输出",
      "schema": {
        "type": "any"
      },
      "rels": ["returnValue"]
    },
    {
      "id": "resetValue",
      "title": "重置值",
      "desc": "清空富文本输入框的值",
      "schema": {
        "type": "any"
      },
      "rels":[
        "resetValueDone"
      ]
    },
    {
      "id": "setDisabled",
      "title": "设置禁用",
      "schema": {
        "type": "any"
      },
      "rels": [
        "setDisabledDone"
      ]
    },
    {
      "id": "setEnabled",
      "title": "设置启用",
      "schema": {
        "type": "any"
      },
      "rels": [
        "setEnabledDone"
      ]
    },
    {
      "id": "isEnable",
      "title": "启用/禁用",
      "schema": {
        "type": "boolean"
      },
      "rels": [
        "isEnableDone"
      ]
    },
    {
      "id": "setValidateInfo",
      "title": "设置校验状态",
      "desc": "设置表单项的校验状态和校验失败提示文案",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "type": "enum",
            "items": [
              {
                "type": "string",
                "value": "success"
              },
              {
                "type": "string",
                "value": "error"
              }
            ]
          },
          "help": {
            "type": "string"
          }
        }
      },
      "rels": [
        "setValidateInfoDone"
      ]
    }
  ],
  "outputs": [
    {
      "id": "onInitial",
      "title": "值初始化",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onChange",
      "title": "值更新",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onBlur",
      "title": "失去焦点",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onValidate",
      "title": "校验触发",
      "schema": {
        "title": "当前值",
        "type": "string"
      }
    },
    {
      "id": "returnValidate",
      "title": "校验结果",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "title": "校验状态",
            "type": "string"
          },
          "help": {
            "title": "校验提示",
            "type": "string"
          }
        }
      }
    },
    {
      "id": "returnValue",
      "title": "值输出",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setValueDone",
      "title": "设置值完成",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setInitialValueDone",
      "title": "设置初始值完成",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "resetValueDone",
      "title": "重置完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setDisabledDone",
      "title": "禁用完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setEnabledDone",
      "title": "启用完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "isEnableDone",
      "title": "启用/禁用完成",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "setValidateInfoDone",
      "title": "设置校验状态完成",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "type": "enum",
            "items": [
              {
                "type": "string",
                "value": "success"
              },
              {
                "type": "string",
                "value": "error"
              }
            ]
          },
          "help": {
            "type": "string"
          }
        }
      }
    }
  ],
  "schema": ["mybricks.normal-pc.form-container/form-item", "*"]
}
