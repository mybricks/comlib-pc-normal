{
  "title": "开关",
  "namespace": "mybricks.normal-pc.switch",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "version": "1.0.4",
  "description": "开关选择器",
  "icon": "./icon.svg",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.tsx",
  "upgrade": "./upgrade.ts",
  "target": {
    "toReact": "./toReact.ts"
  },
  "inputs": [
    {
      "id": "setValue",
      "title": "设置数据",
      "desc": "设置开关的值，并触发值变化事件输出",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "setInitialValue",
      "title": "设置初始值",
      "desc": "设置开关的初始值，并触发初始化事件输出",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "validate",
      "title": "校验",
      "desc": "触发开关的校验检查，并输出校验结果",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValidate"
      ]
    },
    {
      "id": "getValue",
      "title": "获取数据",
      "desc": "获取开关的值并输出",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValue"
      ]
    },
    {
      "id": "resetValue",
      "title": "重置数据",
      "desc": "清空开关的值",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setDisabled",
      "title": "设置禁用",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setEnabled",
      "title": "设置启用",
      "schema": {
        "type": "any"
      }
    }
  ],
  "outputs": [
    {
      "id": "onInitial",
      "title": "初始化",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "onChange",
      "title": "值更新",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "returnValidate",
      "title": "校验结果",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "title": "校验状态",
            "type": "string"
          },
          "help": {
            "title": "校验提示",
            "type": "string"
          }
        }
      }
    },
    {
      "id": "returnValue",
      "title": "值输出",
      "schema": {
        "type": "boolean"
      }
    }
  ],
  "schema": [
    "mybricks.normal-pc.form-container/form-item",
    "*"
  ]
}