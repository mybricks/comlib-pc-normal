{
  "title": "多选框",
  "namespace": "mybricks.normal-pc.checkbox",
  "version": "1.1.23",
  "description": "多选框",
  "author": "ZhuLin",
  "author_name": "朱琳",
  "icon": "./icon.svg",
  "preview": "https://f2.eckwai.com/kos/nlav11092/u_bntrt5.c5fcf109dc97a785.png",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.ts",
  "upgrade": "./upgrade.ts",
  "target": {
    "toReact": "./toReact.ts"
  },
  "inputs": [
    {
      "id": "setValue",
      "title": "设置值",
      "desc": "设置多选框的值，并触发值变化事件输出",
      "schema": {
        "type": "array",
        "description": "设置对应选中项的值",
        "items": {
          "type": "string"
        }
      },
      "rels": [
        "setValueDone"
      ]
    },
    {
      "id": "setInitialValue",
      "title": "设置初始值",
      "desc": "设置多选框的初始值，并触发初始化事件输出",
      "schema": {
        "type": "array",
        "description": "设置对应选中项的初始值",
        "items": {
          "type": "string"
        }
      },
      "rels": [
        "setInitialValueDone"
      ]
    },
    {
      "id": "validate",
      "title": "校验",
      "desc": "触发多选框的校验检查，并输出校验结果",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValidate"
      ]
    },
    {
      "id": "getValue",
      "title": "获取值",
      "desc": "获取多选框的值并输出",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValue"
      ]
    },
    {
      "id": "resetValue",
      "title": "重置值",
      "desc": "清空多选框的值",
      "schema": {
        "type": "any"
      },
      "rels": [
        "resetValueDone"
      ]
    },
    {
      "id": "setOptions",
      "title": "设置数据源",
      "desc": "设置多选框的选项列表",
      "schema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "label": {
              "title": "标签",
              "description": "选项标签（一般展示）",
              "type": "string"
            },
            "value": {
              "title": "值",
              "description": "选项值",
              "type": "string"
            },
            "disabled": {
              "title": "禁用",
              "description": "选项禁用，true时禁用",
              "type": "boolean"
            },
            "checked": {
              "title": "选中",
              "description": "选项禁用，true时禁用",
              "type": "boolean"
            }
          }
        }
      },
      "rels": [
        "setOptionsDone"
      ]
    },
    {
      "id": "setDisabled",
      "title": "设置禁用",
      "desc": "设置多选框禁用，不能选择",
      "schema": {
        "type": "any"
      },
      "rels": [
        "setDisabledDone"
      ]
    },
    {
      "id": "setEnabled",
      "title": "设置启用",
      "desc": "设置多选框启用，能选择",
      "schema": {
        "type": "any"
      },
      "rels": [
        "setEnabledDone"
      ]
    },
    {
      "id": "isEnable",
      "title": "启用/禁用",
      "desc": "根据输入值决定多选框启用/禁用，true-启用，false-禁用",
      "schema": {
        "type": "boolean"
      },
      "rels": [
        "isEnableDone"
      ]
    },
    {
      "id": "isEditable",
      "title": "设置编辑/只读",
      "desc": "设置表单项编辑/只读态",
      "schema": {
        "type": "boolean"
      },
      "rels": [
        "isEditableDone"
      ]
    },
    {
      "id": "setActiveFontColor",
      "title": "设置激活选项字体的颜色",
      "desc": "设置激活选项字体的颜色",
      "schema": {
        "type": "string"
      },
      "rels": [
        "setActiveFontColorDone"
      ]
    },
    {
      "id": "setDynamicStyles",
      "title": "设置选项样式",
      "desc": "动态设置选项样式",
      "schema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "value": {
              "type": "any",
              "description": "设置动态样式的对应选项值"
            },
            "style": {
              "type": "object",
              "description": "自定义样式"
            }
          }
        }
      },
      "rels": [
        "setDynamicStylesDone"
      ]
    },
    {
      "id": "setValidateInfo",
      "title": "设置校验状态",
      "desc": "设置表单项的校验状态和校验失败提示文案",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "type": "enum",
            "descriptions": "校验状态，成功/失败",
            "items": [
              {
                "type": "string",
                "value": "success"
              },
              {
                "type": "string",
                "value": "error"
              }
            ]
          },
          "help": {
            "type": "string"
          }
        }
      },
      "rels": [
        "setValidateInfoDone"
      ]
    }
  ],
  "outputs": [
    {
      "id": "onInitial",
      "title": "值初始化",
      "desc": "多选框值初始化时触发",
      "schema": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    {
      "id": "onChange",
      "title": "值更新",
      "desc": "多选框值更新时触发",
      "schema": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    {
      "id": "onValidate",
      "title": "校验触发",
      "desc": "多选框触发校验时触发",
      "schema": {
        "title": "当前值",
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    {
      "id": "returnValidate",
      "title": "校验结果",
      "desc": "多选框输出校验结果",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "title": "校验状态",
            "description": "校验状态，成功/失败",
            "type": "string"
          },
          "help": {
            "title": "校验提示",
            "description": "帮助提示文案",
            "type": "string"
          }
        }
      }
    },
    {
      "id": "returnValue",
      "title": "值输出",
      "desc": "获取多选框值后，值输出",
      "schema": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    {
      "id": "setValueDone",
      "title": "设置值完成",
      "schema": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    {
      "id": "setInitialValueDone",
      "title": "设置初始值完成",
      "schema": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    {
      "id": "resetValueDone",
      "title": "重置完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setOptionsDone",
      "title": "设置数据源完成",
      "schema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "label": {
              "title": "标签",
              "description": "选项标签（一般展示）",
              "type": "string"
            },
            "value": {
              "title": "值",
              "description": "选项值",
              "type": "string"
            },
            "disabled": {
              "title": "禁用",
              "description": "选项禁用，true时禁用",
              "type": "boolean"
            },
            "checked": {
              "title": "选中",
              "description": "选项禁用，true时禁用",
              "type": "boolean"
            }
          }
        }
      }
    },
    {
      "id": "setDisabledDone",
      "title": "禁用完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setEnabledDone",
      "title": "启用完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "isEnableDone",
      "title": "启用/禁用完成",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "isEditableDone",
      "title": "设置编辑/只读完成",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "setActiveFontColorDone",
      "title": "设置激活选项字体的颜色完成",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setValidateInfoDone",
      "title": "设置校验状态完成",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "type": "enum",
            "descriptions": "校验状态，成功/失败",
            "items": [
              {
                "type": "string",
                "value": "success"
              },
              {
                "type": "string",
                "value": "error"
              }
            ]
          },
          "help": {
            "type": "string"
          }
        }
      }
    },
    {
      "id": "setOptionsDone",
      "title": "设置数据源完成",
      "schema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "label": {
              "title": "标签",
              "description": "选项标签（一般展示）",
              "type": "string"
            },
            "value": {
              "title": "值",
              "description": "选项值",
              "type": "string"
            },
            "disabled": {
              "title": "禁用",
              "description": "选项禁用，true时禁用",
              "type": "boolean"
            },
            "checked": {
              "title": "选中",
              "description": "选项禁用，true时禁用",
              "type": "boolean"
            }
          }
        }
      }
    }
  ],
  "schema": [
    "mybricks.normal-pc.form-container/form-item",
    "*"
  ]
}