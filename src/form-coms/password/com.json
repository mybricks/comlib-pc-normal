{
  "title": "密码框",
  "namespace": "mybricks.normal-pc.password",
  "author": "tangxiaoxin",
  "author_name": "唐小新",
  "version": "1.0.3",
  "description": "密码输入框",
  "docs": "组件使用说明文档链接",
  "icon": "./icon.svg",
  "runtime": "./runtime.tsx",
  "data": "./data.json",
  "editors": "./editors.ts",
  "upgrade": "./upgrade.ts",
  "target": {
    "toReact": "./toReact.ts"
  },
  "inputs": [
    {
      "id": "setValue",
      "title": "设置值",
      "schema": {
        "type": "string"
      },
      "desc": "设置密码"
    },
    {
      "id": "setInitialValue",
      "title": "设置初始值",
      "desc": "设置密码框初始值，并触发初始化事件输出",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "getValue",
      "title": "获取值",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValue"
      ],
      "desc": "获取密码"
    },
    {
      "id": "resetValue",
      "title": "重置值",
      "schema": {
        "type": "any"
      },
      "desc": "重置密码框"
    },
    {
      "id": "validate",
      "title": "校验",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValidate"
      ]
    },
    {
      "id": "setDisabled",
      "title": "设置禁用",
      "schema": {
        "type": "any"
      },
      "desc": "禁用密码输入框"
    },
    {
      "id": "setEnabled",
      "title": "设置启用",
      "schema": {
        "type": "any"
      },
      "desc": "启用密码输入框"
    }
  ],
  "outputs": [
    {
      "id": "onInitial",
      "title": "初始化",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "returnValue",
      "title": "数据输出",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "returnValidate",
      "title": "校验结果",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "title": "校验状态",
            "type": "string"
          },
          "help": {
            "title": "校验提示",
            "type": "string"
          }
        }
      }
    },
    {
      "id": "onChange",
      "title": "值发生改变",
      "schema": {
        "type": "string"
      }
    }
  ],
  "slots": [],
  "schema": [
    "mybricks.normal-pc.form-container/form-item",
    "*"
  ]
}