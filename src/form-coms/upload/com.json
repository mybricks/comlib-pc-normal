{
  "title": "上传",
  "namespace": "mybricks.normal-pc.upload",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "version": "1.0.50",
  "description": "上传",
  "icon": "./icon.svg",
  "preview": "https://f2.eckwai.com/kos/nlav11092/u_bj3zsx.0442800137cf236a.png",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.tsx",
  "upgrade": "./upgrade.ts",
  "target": {
    "toReact": "./toReact.ts"
  },
  "inputs": [
    {
      "id": "setValue",
      "title": "设置值",
      "desc": "设置上传表单项的值",
      "schema": {
        "type": "array",
        "description": "设置上传表单项的值",
        "items": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string",
              "description": "文件名"
            },
            "url": {
              "type": "string",
              "description": "文件地址"
            },
            "status": {
              "type": "string",
              "description": "文件上传状态"
            },
            "percent": {
              "type": "number",
              "description": "文件上传进度，百分比"
            },
            "response": {
              "type": "string",
              "description": "服务端响应内容"
            }
          }
        }
      },
      "rels": [
        "setValueDone"
      ]
    },
    {
      "id": "setInitialValue",
      "title": "设置初始值",
      "desc": "设置上传表单项的初始值，并触发值初始化事件输出",
      "schema": {
        "type": "array",
        "description": "设置上传表单项的值",
        "items": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string",
              "description": "文件名"
            },
            "url": {
              "type": "string",
              "description": "文件地址"
            },
            "status": {
              "type": "string",
              "description": "文件上传状态"
            },
            "percent": {
              "type": "number",
              "description": "文件上传进度，百分比"
            },
            "response": {
              "type": "string",
              "description": "服务端响应内容"
            }
          }
        }
      },
      "rels": [
        "setInitialValueDone"
      ]
    },
    {
      "id": "validate",
      "title": "校验",
      "desc": "触发上传表单项的校验检查，并输出校验结果",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValidate"
      ]
    },
    {
      "id": "getValue",
      "title": "获取值",
      "desc": "获取上传表单项的值，并输出",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValue"
      ]
    },
    {
      "id": "resetValue",
      "title": "重置值",
      "desc": "清空上传表单项的值",
      "schema": {
        "type": "any"
      },
      "rels": [
        "resetValueDone"
      ]
    },
    {
      "id": "setDisabled",
      "title": "设置禁用",
      "desc": "禁用上传表单项，并触发禁用完成事件输出",
      "schema": {
        "type": "any"
      },
      "rels": [
        "setDisabledDone"
      ]
    },
    {
      "id": "setEnabled",
      "title": "设置启用",
      "desc": "启用上传表单项，并触发启用完成事件输出",
      "schema": {
        "type": "any"
      },
      "rels": [
        "setEnabledDone"
      ]
    },
    {
      "id": "isEnable",
      "title": "启用/禁用",
      "desc": "对上传表单项进行启用/禁用状态设置，并触发启用/禁用完成事件",
      "schema": {
        "type": "boolean"
      },
      "rels": [
        "isEnableDone"
      ]
    },
    {
      "id": "isEditable",
      "title": "设置编辑/只读",
      "desc": "设置表单项编辑/只读态",
      "schema": {
        "type": "boolean"
      },
      "rels": [
        "isEditableDone"
      ]
    },
    {
      "id": "remove",
      "title": "删除文件",
      "desc": "删除上传表单项的文件，并触发删除文件完成事件输出",
      "schema": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "文件名"
          },
          "uid": {
            "type": "string",
            "description": "文件唯一标识符"
          }
        }
      },
      "rels": [
        "removeDone"
      ]
    }
  ],
  "outputs": [
    {
      "id": "onInitial",
      "title": "值初始化",
      "desc": "上传表单项的值初始化事件输出",
      "schema": {
        "type": "array",
        "description": "值初始化后，输出上传表单项的值",
        "items": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string",
              "description": "文件名"
            },
            "url": {
              "type": "string",
              "description": "文件地址"
            },
            "status": {
              "type": "string",
              "description": "文件上传状态"
            },
            "percent": {
              "type": "number",
              "description": "文件上传进度，百分比"
            },
            "response": {
              "type": "string",
              "description": "服务端响应内容"
            }
          }
        }
      }
    },
    {
      "id": "returnValidate",
      "title": "校验结果",
      "desc": "上传表单项输出校验结果",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "title": "校验状态，成功/失败",
            "type": "string"
          },
          "help": {
            "title": "帮助提示信息",
            "type": "string"
          }
        }
      }
    },
    {
      "id": "returnValue",
      "title": "值输出",
      "desc": "获取上传表单项的值后，值输出",
      "schema": {
        "type": "array",
        "description": "输出上传表单项的值",
        "items": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string",
              "description": "文件名"
            },
            "url": {
              "type": "string",
              "description": "文件地址"
            },
            "status": {
              "type": "string",
              "description": "文件上传状态"
            },
            "percent": {
              "type": "number",
              "description": "文件上传进度，百分比"
            },
            "response": {
              "type": "string",
              "description": "服务端响应内容"
            }
          }
        }
      }
    },
    {
      "id": "remove",
      "title": "删除文件",
      "desc": "删除上传表单项的文件后，触发删除文件事件",
      "schema": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "文件名"
          },
          "uid": {
            "type": "string",
            "description": "文件唯一标识符"
          },
          "url": {
            "type": "string",
            "description": "文件地址"
          },
          "status": {
            "type": "string",
            "description": "文件上传状态"
          },
          "percent": {
            "type": "number",
            "description": "文件上传进度，百分比"
          },
          "response": {
            "type": "string",
            "description": "服务端响应内容"
          }
        }
      }
    },
    {
      "id": "setValueDone",
      "title": "设置值完成",
      "schema": {
        "type": "array",
        "description": "输出上传表单项的值",
        "items": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string",
              "description": "文件名"
            },
            "url": {
              "type": "string",
              "description": "文件地址"
            },
            "status": {
              "type": "string",
              "description": "文件上传状态"
            },
            "percent": {
              "type": "number",
              "description": "文件上传进度，百分比"
            },
            "response": {
              "type": "string",
              "description": "服务端响应内容"
            }
          }
        }
      }
    },
    {
      "id": "setInitialValueDone",
      "title": "设置初始值完成",
      "schema": {
        "type": "array",
        "description": "输出上传表单项的值",
        "items": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string",
              "description": "文件名"
            },
            "url": {
              "type": "string",
              "description": "文件地址"
            },
            "status": {
              "type": "string",
              "description": "文件上传状态"
            },
            "percent": {
              "type": "number",
              "description": "文件上传进度，百分比"
            },
            "response": {
              "type": "string",
              "description": "服务端响应内容"
            }
          }
        }
      }
    },
    {
      "id": "resetValueDone",
      "title": "重置完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setDisabledDone",
      "title": "禁用完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setEnabledDone",
      "title": "启用完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "isEnableDone",
      "title": "启用/禁用完成",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "isEditableDone",
      "title": "设置编辑/只读完成",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "uploadComplete",
      "title": "上传完成后",
      "schema": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
