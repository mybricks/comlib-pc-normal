{
  "title": "上传",
  "namespace": "mybricks.normal-pc.upload",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "version": "1.0.0",
  "description": "上传",
  "icon": "./icon.svg",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.tsx",
  "inputs": [
    {
      "id": "setValue",
      "title": "设置值",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "validate",
      "title": "校验",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValidate"
      ]
    },
    {
      "id": "getValue",
      "title": "获取值",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValue"
      ]
    },
    {
      "id": "resetValue",
      "title": "重置值",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setDisabled",
      "title": "设置禁用",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "setEnabled",
      "title": "设置启用",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "uploadDone",
      "title": "上传完成",
      "schema": {
        "type": "follow"
      }
    }
  ],
  "outputs": [
    {
      "id": "onChange",
      "title": "值发生改变",
      "schema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "uid": {
              "type": "string"
            },
            "name": {
              "type": "string"
            },
            "status": {
              "type": "string"
            },
            "url": {
              "type": "string"
            }
          }
        }
      }
    },
    {
      "id": "returnValidate",
      "title": "校验结果",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "title": "校验状态",
            "type": "string"
          },
          "help": {
            "title": "校验提示",
            "type": "string"
          }
        }
      }
    },
    {
      "id": "returnValue",
      "title": "值输出",
      "schema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "uid": {
              "type": "string"
            },
            "name": {
              "type": "string"
            },
            "status": {
              "type": "string"
            },
            "url": {
              "type": "string"
            }
          }
        }
      }
    },
    {
      "id": "upload",
      "title": "开始上传",
      "schema": {
        "type": "any"
      }
    }
  ],
  "schema": [
    "mybricks.normal-pc.form-container/form-item",
    "*"
  ]
}