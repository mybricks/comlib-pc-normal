{
  "title": "数字输入框",
  "namespace": "mybricks.normal-pc.input-number",
  "author": "HuangQiuyun",
  "author_name": "黄秋云",
  "version": "1.0.3",
  "description": "数字输入框",
  "icon": "./icon.svg",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.ts",
  "upgrade": "./upgrade.ts",
  "target": {
    "toReact": "./toReact.ts"
  },
  "inputs": [
    {
      "id": "setValue",
      "title": "设置值",
      "desc": "设置数字输入框的值，并触发值变化事件输出",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "setInitialValue",
      "title": "设置初始值",
      "desc": "设置数字输入框的初始值，并触发初始化事件输出",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "validate",
      "title": "校验",
      "desc": "触发数字输入框的校验检查，并输出校验结果",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValidate"
      ]
    },
    {
      "id": "getValue",
      "title": "获取值",
      "desc": "获取数字输入框的值并输出",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValue"
      ]
    },
    {
      "id": "resetValue",
      "title": "重置值",
      "desc": "清空数字输入框的值",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setDisabled",
      "title": "设置禁用",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setEnabled",
      "title": "设置启用",
      "schema": {
        "type": "any"
      }
    }
  ],
  "outputs": [
    {
      "id": "onInitial",
      "title": "初始化",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "onChange",
      "title": "值变化",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "onBlur",
      "title": "失去焦点",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "onPressEnter",
      "title": "按下回车",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "returnValidate",
      "title": "校验结果",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "title": "校验状态",
            "type": "string"
          },
          "help": {
            "title": "校验提示",
            "type": "string"
          }
        }
      }
    },
    {
      "id": "returnValue",
      "title": "值输出",
      "schema": {
        "type": "number"
      }
    }
  ],
  "schema": [
    "mybricks.normal-pc.form-container/form-item",
    "*"
  ]
}