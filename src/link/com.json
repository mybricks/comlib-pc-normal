{
  "title": "链接",
  "namespace": "mybricks.normal-pc.link",
  "version": "1.0.10",
  "description": "链接",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "icon": "./icon.svg",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.tsx",
  "upgrade": "./upgrade.ts",
  "inputs": [
    {
      "id": "content",
      "title": "文本内容",
      "desc": "设置链接文本内容，并触发设置文本内容完成事件输出",
      "schema": {
        "type": "string"
      },
      "rels": [
        "setContentDone"
      ]
    },
    {
      "id": "url",
      "title": "链接地址",
      "desc": "设置链接的跳转地址，并触发设置链接地址完成事件输出",
      "schema": {
        "type": "string"
      },
      "rels": [
        "setUrlDone"
      ]
    }
  ],
  "outputs": [
    {
      "id": "setContentDone",
      "title": "设置文本内容完成",
      "desc": "链接设置文本内容后，事件输出",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setUrlDone",
      "title": "设置链接地址完成",
      "desc": "链接的设置链接地址完成后，事件输出",
      "schema": {
        "type": "string"
      }
    }
  ]
}