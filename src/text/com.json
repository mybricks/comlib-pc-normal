{
  "title": "文本",
  "namespace": "mybricks.normal-pc.text",
  "version": "1.0.19",
  "description": "文本",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "icon": "./icon.svg",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.tsx",
  "upgrade": "./upgrade.ts",
  "ai": "./ai.ts",
  "target": {
    "toReact": "./toReact.ts"
  },
  "inputs": [
    {
      "id": "content",
      "title": "内容",
      "desc": "设置文本内容，并关联设置内容完成输出",
      "schema": {
        "type": "string"
      },
      "rels": [
        "setContentDone"
      ]
    }
  ],
  "outputs": [
    {
      "id": "setContentDone",
      "title": "设置内容完成",
      "desc": "文本内容设置完成，并输出内容",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "click",
      "title": "点击",
      "desc": "文本内容点击，输出文本内容",
      "schema": {
        "type": "string"
      }
    }
  ]
}